0435\u043C\u044B \u0418 \u0443\u043F\u043E\u043C\u044F\u043D\u0443\u0442\u044B\u0439 \u0432 \u043D\u0438\u0445 \u0432\u043E\u043F\u0440\u043E\u0441 \u0443\u0436\u0435 \u0440\u0430\u0437\u0440\u0435\u0448\u0435\u043D,\u003Cbr\u003E\n- \u043E\u043D\u0438 \u0440\u0435\u0437\u043A\u043E \u043D\u0435 \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044E\u0442 \u0437\u0430\u0434\u0430\u0447\u0430\u043C \u0442\u0435\u043C\u044B \u0418 \u0432 \u043D\u0438\u0445 \u043E\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u043F\u0440\u0435\u0434\u043C\u0435\u0442 \u043E\u0431\u0441\u0443\u0436\u0434\u0435\u043D\u0438\u044F,\u003Cbr\u003E\n- \u043E\u043D\u0438 \u0441\u043E\u0434\u0435\u0440\u0436\u0430\u0442 \u0433\u0440\u0443\u0431\u0435\u0439\u0448\u0438\u0435 \u043D\u0430\u0440\u0443\u0448\u0435\u043D\u0438\u044F (\u043C\u043E\u0434\u0435\u0440\u0430\u0442\u043E\u0440\u044B \u043C\u043E\u0433\u0443\u0442 \u0443\u0434\u0430\u043B\u044F\u0442\u044C \u044D\u0442\u0438 \u043D\u0430\u0440\u0443\u0448\u0435\u043D\u0438\u044F).\n\u003C/p\u003E\n\u003Cp\u003E\n\u042D\u0442\u0443 \u0442\u0435\u043C\u0443 \u043C\u043E\u0436\u043D\u043E \u043F\u043E\u043D\u0438\u043C\u0430\u0442\u044C \u043A\u0430\u043A \u0022\u043F\u0430\u0440\u043A\u043E\u0432\u043E\u0447\u043D\u0443\u044E\u0022 \u043F\u043B\u043E\u0449\u0430\u0434\u043A\u0443 \u0434\u043B\u044F \u0442\u0430\u043A\u0438\u0445 \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439.\n\u003C/p\u003E\n\u003Cp\u003E\n\u003Cstrong\u003E\u041D\u0438\u0447\u0435\u0433\u043E \u043F\u043B\u043E\u0445\u043E\u0433\u043E \u0432 \u043F\u043E\u044F\u0432\u043B\u0435\u043D\u0438\u0438 \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439 \u0442\u0443\u0442 \u043D\u0435\u0442.\u003Cbr\u003E\n\u041E\u0431\u044B\u0447\u043D\u043E \u044D\u0442\u0438 \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u044F \u0443\u0436\u0435 \u0441\u0435\u0431\u044F \u0438\u0441\u0447\u0435\u0440\u043F\u0430\u043B\u0438, \u043D\u043E \u0022\u043A\u0430\u043A \u0435\u0441\u0442\u044C\u0022 \u043D\u0435 \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044E\u0442 \u0438\u043C\u0435\u044E\u0449\u0438\u043C\u0441\u044F \u0442\u0435\u043C\u0430\u043C \u043D\u0430 \u0444\u043E\u0440\u0443\u043C\u0435.\u003C/strong\u003E\n\u003C/p\u003E\n\u003Cp\u003E\n\u0412\u043E\u0437\u043C\u043E\u0436\u043D\u043E, \u0432\u0430\u043C \u0431\u0443\u0434\u0435\u0442 \u043D\u0435\u043F\u0440\u0438\u044F\u0442\u043D\u043E \u043E\u0431\u043D\u0430\u0440\u0443\u0436\u0438\u0442\u044C \u0441\u0432\u043E\u0435 \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435 \u0442\u0443\u0442, - \u043D\u043E \u0441\u043E\u0433\u043B\u0430\u0441\u0438\u0442\u0435\u0441\u044C \u044D\u0442\u043E \u043B\u0443\u0447\u0448\u0435, \u0447\u0435\u043C \u0435\u0441\u043B\u0438 \u043E\u043D\u0438 \u0431\u0443\u0434\u0443\u0442 \u0441\u043A\u0440\u044B\u0442\u044B, \u0438\u043B\u0438 \u043E\u043D\u0438 \u0431\u0443\u0434\u0443\u0442 \u043B\u0435\u0436\u0430\u0442\u044C \u0438 \u043C\u0435\u0448\u0430\u0442\u044C \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044E \u0442\u0435\u043C\u044B. \u003Cu\u003E\u0415\u0441\u043B\u0438 \u0432\u044B \u043D\u0435\u0441\u043E\u0433\u043B\u0430\u0441\u043D\u044B\u003C/u\u003E \u0441 \u043F\u0435\u0440\u0435\u043D\u043E\u0441\u043E\u043C \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439 \u0441\u044E\u0434\u0430, \u0432\u044B \u0430\u0431\u0441\u043E\u043B\u044E\u0442\u043D\u043E \u0432 \u043F\u0440\u0430\u0432\u0435 \u0441\u043F\u0440\u043E\u0441\u0438\u0442\u044C \u043C\u043E\u0434\u0435\u0440\u0430\u0442\u043E\u0440\u0430 \u043E \u043F\u0440\u0438\u0447\u0438\u043D\u0430\u0445 \u043F\u0435\u0440\u0435\u043D\u043E\u0441\u0430, \u0442\u0440\u0435\u0431\u043E\u0432\u0430\u043D\u0438\u044F\u0445 \u0434\u043B\u044F \u0432\u043E\u0437\u0432\u0440\u0430\u0442\u0430 \u0438 \u043F\u043E\u043F\u0440\u043E\u0441\u0438\u0442\u044C \u043F\u0435\u0440\u0435\u043D\u0435\u0441\u0442\u0438 \u043E\u0431\u0440\u0430\u0442\u043D\u043E - \u0434\u043B\u044F \u044D\u0442\u043E\u0433\u043E \u043E\u0442\u043F\u0440\u0430\u0432\u044C\u0442\u0435 \u043C\u043E\u0434\u0435\u0440\u0430\u0442\u043E\u0440\u0443 \u003Cu\u003E\u041B\u0438\u0447\u043D\u043E\u0435 \u0421\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435\u003C/u\u003E.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0412\u044B \u043C\u043E\u0436\u0435\u0442\u0435 \u0441\u043A\u043E\u0440\u0440\u0435\u043A\u0442\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0435 \u043F\u0440\u044F\u043C\u043E \u0442\u0443\u0442 \u0438 \u043F\u043E\u043F\u0440\u043E\u0441\u0438\u0442\u044C \u0432\u0435\u0440\u043D\u0443\u0442\u044C \u0432 \u0442\u0435\u043C\u0443. \u0418\u043B\u0438 \u0436\u0435 \u0441\u043E\u0437\u0434\u0430\u0442\u044C \u043D\u043E\u0432\u043E\u0435, \u0438\u0441\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u043D\u043E\u0435.\n\u003C/p\u003E\n\u003Cp\u003E\n\u041E\u0431\u044B\u0447\u043D\u043E \u043C\u043E\u0434\u0435\u0440\u0430\u0442\u043E\u0440 \u0432\u0441\u0435\u0433\u0434\u0430 \u0431\u0443\u0434\u0435\u0442 \u0443\u043A\u0430\u0437\u044B\u0432\u0430\u0442\u044C \u0444\u0430\u043A\u0442 \u0438 \u043F\u0440\u0438\u0447\u0438\u043D\u0443 \u043F\u0435\u0440\u0435\u043D\u043E\u0441\u0430 \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439 \u0432 \u0438\u0441\u0445\u043E\u0434\u043D\u043E\u0439 \u0442\u0435\u043C\u0435.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0421\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u044F \u0434\u043E\u0431\u0430\u0432\u043B\u044F\u044E\u0442\u0441\u044F \u043F\u043E \u0434\u0430\u0442\u0435 \u043F\u0443\u0431\u043B\u0438\u043A\u043E\u0432\u0430\u043D\u0438\u044F, \u043F\u043E\u044D\u0442\u043E\u043C\u0443 \u003Cu\u003E\u0438\u0445 \u043F\u043E\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u0442\u0435\u043B\u044C\u043D\u043E\u0441\u0442\u044C \u0432 \u044D\u0442\u043E\u0439 \u0442\u0435\u043C\u0435 \u043D\u0435 \u0433\u0430\u0440\u0430\u043D\u0442\u0438\u0440\u0443\u0435\u0442\u0441\u044F\u003C/u\u003E (\u044D\u0442\u043E \u043D\u043E\u0440\u043C\u0430\u043B\u044C\u043D\u043E). \u041F\u043E\u0441\u043B\u0435 \u0432\u043E\u0437\u0432\u0440\u0430\u0442\u0430 - \u043F\u043E\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u0442\u0435\u043B\u044C\u043D\u043E\u0441\u0442\u044C \u0431\u0443\u0434\u0435\u0442 \u0432\u043E\u0441\u0441\u0442\u0430\u043D\u043E\u0432\u043B\u0435\u043D\u0430.\u003Cbr\u003E\n\u0412 \u044D\u0442\u043E\u0439 \u0442\u0435\u043C\u0435 \u0432\u044B \u0442\u0430\u043A\u0436\u0435 \u043C\u043E\u0436\u0435\u0442\u0435 \u0441\u043C\u0435\u043B\u043E \u0442\u0440\u0435\u043D\u0438\u0440\u043E\u0432\u0430\u0442\u044C\u0441\u044F \u0441 \u0441\u043E\u0441\u0442\u0430\u0432\u043B\u0435\u043D\u0438\u0438 \u0441\u043E\u043E\u0431\u0449\u0435\u043D\u0438\u0439!\n\u003C/p\u003E\n"},{"CreatedByName":"WovKino Studio","CreatedById":182584,"CreatedDateTime":"2018-03-13T15:55:17Z","Content":"\n\u003Cp\u003E\n\u003Cstrong\u003E\u0418\u043C\u044F:\u003C/strong\u003E\u00A0 \u003Cem\u003ERELAY DUBLETT\u003C/em\u003E\u003Cbr\u003E\u003Cstrong\u003E\u0422\u0438\u043F:\u003C/strong\u003E \u003Cem\u003EC\u0438\u0441\u0442\u0435\u043C\u0430 \u0441\u0432\u044F\u0437\u0438\u003C/em\u003E\u003Cbr\u003E\u003Cstrong\u003E\u0412\u0435\u0440\u0441\u0438\u044F \u041A\u0421\u041F:\u003C/strong\u003E \u003Cem\u003E1.4\u003C/em\u003E\u003Cbr\u003E\u003Cstrong\u003E\u041C\u043E\u0434\u044B: \u0421\u0442\u043E\u043A\u002B \u0442\u043E\u043B\u044C\u043A\u043E \u003Cabbr title=\u0022Mechjeb (mod)\u0022\u003EMJ\u003C/abbr\u003E\u003C/strong\u003E\u003Cbr\u003E\u003Cstrong\u003E\u0413\u0440\u0443\u043F\u043F\u044B \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0439:\u003C/strong\u003E\u00A0 \u003Cem\u003E,\u003C/em\u003E\u003Cbr\u003E\n1 \u0430\u043D\u0442\u0435\u043D\u043D\u044B \u0440\u0430\u0437 \u0442\u0443\u0434\u044B-\u0441\u044E\u0434\u044B\n\u003C/p\u003E\n\u003Cp\u003E\n2 \u0430\u043D\u0442\u0435\u043D\u043D\u044B \u0434\u0432\u0430\u0441 \u0442\u0443\u0434\u044B-\u0441\u044E\u0434\u044B\n\u003C/p\u003E\n\u003Cp\u003E\n3 \u043F\u0443\u0441\u0442\u043E\n\u003C/p\u003E\n\u003Cp\u003E\n4 \u0441\u043E\u043B\u043D\u0435\u0447\u043D\u044B\u0435 \u0431\u0430\u0442\u0430\u0440\u0435\u0438 \u0442\u0443\u0434\u044B-\u0441\u044E\u0434\u044B (\u043D\u0430 \u043C\u0430\u043B\u043E\u043C \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u0435 \u0432\u0440\u0443\u0447\u043D\u0443\u044E)\u003Cbr\u003E\u003Cstrong\u003E\u041E\u0431\u043B\u0430\u0441\u0442\u044C \u043F\u0440\u0438\u043C\u0435\u043D\u0435\u043D\u0438\u044F:\u003C/strong\u003E\u00A0 \u003Cem\u003E\u043F\u0430\u0440\u0430 \u0434\u043B\u044F \u0441\u043E\u0437\u0434\u0430\u043D\u0438\u044F \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u043E\u0432\u043E\u0439 \u0433\u0440\u0443\u043F\u043F\u0438\u0440\u043E\u0432\u043A\u0438 \u0441\u0432\u044F\u0437\u0438\u003C/em\u003E\u003Cbr\u003E\u003Cstrong\u003E\u0418\u0441\u0442\u043E\u0440\u0438\u044F \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u043D\u0438\u044F/\u041C\u0438\u0441\u0441\u0438\u0438:\u003C/strong\u003E\u003Cem\u003E \u043F\u043E\u0437\u0432\u043E\u043B\u044F\u0435\u0442 \u043B\u0435\u0433\u043E \u0440\u0430\u0437\u043C\u0435\u0441\u0442\u0438\u0442\u044C \u043F\u0430\u0440\u0443 \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u043E\u0432 \u0441\u0432\u044F\u0437\u0438,\u003C/em\u003E\u003Cem\u003E\u043E\u0434\u0438\u043D \u043D\u0430 \u0432\u043D\u0435\u0448\u043D\u0435\u0439 \u043E\u0440\u0431\u0438\u0442\u0435 \u041B\u0443\u043D\u044B \u0434\u0440\u0443\u0433\u043E\u0439 \u043D\u0430 \u043D\u0438\u0437\u043A\u043E\u0439 (25 \u043A\u043C)\u003C/em\u003E\u003Cbr\u003E\u003Cstrong\u003E\u0414\u0430\u043B\u044C\u043D\u043E\u0441\u0442\u044C \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u044F:\u003C/strong\u003E\u003Cem\u003E \u0435\u0441\u043B\u0438 \u0441\u043A\u0443\u0447\u043D\u043E \u043C\u043E\u0436\u043D\u043E \u0438 \u043D\u0430 \u041B\u0443\u043D\u0443 \u043F\u043E\u0441\u0430\u0434\u0438\u0442\u044C\u003C/em\u003E\u003Cbr\u003E