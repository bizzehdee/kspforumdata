\u0445 \u0441\u0430\u0439\u0442\u0430\u0445.\u003Cbr\u003E\n\u041E\u0431 \u044D\u0442\u043E\u043C, \u043D\u0430\u043F\u0440\u0438\u043C\u0435\u0440, \u0433\u043E\u0432\u043E\u0440\u0438\u0442 \u0430\u043A\u0442\u0438\u0432\u043D\u043E\u0435 \u0431\u043B\u043E\u043A\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u0435 \u0441\u043A\u0440\u0438\u043F\u0442\u043E\u043C\u00A0\u043F\u0440\u0430\u0432\u043E\u0439 \u043A\u043D\u043E\u043F\u043A\u043E\u0439 \u043C\u044B\u0448\u0438 (\u041F\u041A\u041C) \u043D\u0430\u0434 \u043A\u0430\u0440\u0442\u0438\u043D\u043A\u043E\u0439. \u0421\u043A\u043E\u0440\u0435\u0439 \u0432\u0441\u0435\u0433\u043E \u044D\u0442\u043E \u0441\u0434\u0435\u043B\u0430\u043D\u043E \u043F\u043E\u0442\u043E\u043C\u0443, \u0447\u0442\u043E \u043E\u043D \u0437\u0430\u0440\u0430\u0431\u0430\u0442\u044B\u0432\u0430\u0435\u0442 \u043D\u0430 \u0440\u0435\u043A\u043B\u0430\u043C\u0435 (\u043F\u0440\u043E\u0432\u0435\u0440\u0438\u0442\u044C \u043D\u0435 \u043C\u043E\u0433\u0443, \u043F\u043E\u0442\u043E\u043C\u0443 \u0447\u0442\u043E \u0443 \u043C\u0435\u043D\u044F \u0435\u0451 \u0440\u0435\u0436\u0435\u0442 ublock\u002Bpi-hole). \u041D\u0430 \u0442\u0430\u043A\u0438\u0445 \u0445\u043E\u0441\u0442\u0430\u0445 \u0447\u0430\u0441\u0442\u043E \u0442\u0430\u043A\u0436\u0435 \u0432\u0441\u0442\u0440\u0435\u0447\u0430\u0435\u0442\u0441\u044F \u043E\u0433\u0440\u0430\u043D\u0438\u0447\u0435\u043D\u0438\u0435 \u043D\u0430 \u0442\u0440\u0430\u0444\u0438\u043A \u0434\u043B\u044F \u0432\u0441\u0442\u0430\u0432\u043E\u043A \u0432\u043E \u0432\u043D\u0435\u0448\u043D\u0438\u0435 \u0440\u0435\u0441\u0443\u0440\u0441\u044B. \u0417\u0430\u0442\u0435\u043C \u0022\u043D\u0435\u043E\u0436\u0438\u0434\u0430\u043D\u043D\u043E\u0022 \u043A\u0430\u0440\u0442\u0438\u043D\u043A\u0438 \u0437\u0430\u043C\u0435\u043D\u044F\u044E\u0442\u0441\u044F \u043D\u0430\u0434\u043F\u0438\u0441\u044F\u043C\u0438-\u043F\u0440\u043E\u0441\u044C\u0431\u0430\u043C\u0438 \u043F\u0435\u0440\u0435\u0432\u0435\u0441\u0442\u0438 \u0443\u0447\u0435\u0442\u043A\u0443 \u043D\u0430 \u043F\u043B\u0430\u0442\u043D\u0443\u044E \u043E\u0441\u043D\u043E\u0432\u0443.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0422\u0435\u043C \u043D\u0435 \u043C\u0435\u043D\u0435\u0435, \u0435\u0441\u043B\u0438 \u043D\u0430 \u0441\u0442\u0440\u0430\u043D\u0438\u0446\u0435 uploads.ru \u0432\u044B \u043E\u0431\u0440\u0430\u0442\u0438\u0442\u0435 \u0432\u043D\u0438\u043C\u0430\u043D\u0438\u0435 \u043D\u0430 \u043F\u043E\u043B\u0435 \u0022HTML\u0022 \u043A\u043E\u0442\u043E\u0440\u0430\u044F \u0432 \u0022\u043F\u0440\u0435\u0432\u044C\u044E\u002B\u0441\u0441\u044B\u043B\u043A\u0430\u0022, - \u0442\u043E \u0432\u043D\u0443\u0442\u0440\u0438 HTML \u043A\u043E\u0434\u0430, \u0441\u0440\u0430\u0437\u0443 \u043F\u043E\u0441\u043B\u0435 \u0022img src=\u0022,\u00A0 \u0432 \u043A\u0430\u0432\u044B\u0447\u043A\u0430\u0445 \u0431\u0443\u0434\u0435\u0442 \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0441\u044F \u043F\u0440\u044F\u043C\u0430\u044F \u0441\u0441\u044B\u043B\u043A\u0430 \u043D\u0430 \u0438\u0437\u043E\u0431\u0440\u0430\u0436\u0435\u043D\u0438\u0435.\u003Cbr\u003E\n\u041E\u043D\u0430 \u0438\u043C\u0435\u0435\u0442 \u0432\u0438\u0434 \u0022http:// ... jpg\u0022\u003Cbr\u003E\n\u0415\u0441\u043B\u0438 \u0432\u0441\u0442\u0430\u0432\u0438\u0442\u044C \u0435\u0451 \u0441\u044E\u0434\u0430 (\u0438 \u0443\u0431\u0440\u0430\u0442\u044C \u0432\u0435\u0441\u044C \u043B\u0438\u0448\u043D\u0438\u0439 \u0442\u0435\u043A\u0441\u0442 \u0432\u043E\u043A\u0440\u0443\u0433, - \u043F\u0440\u044F\u043C\u043E \u043D\u0430 \u0441\u0430\u0439\u0442\u0435 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C \u0447\u0430\u0441\u0442\u0438\u0447\u043D\u043E\u0433\u043E \u0432\u044B\u0431\u043E\u0440\u0430 \u0442\u0430\u043A\u0436\u0435 \u0431\u043B\u043E\u043A\u0438\u0440\u0443\u0435\u0442\u0441\u044F \u0441\u043A\u0440\u0438\u043F\u0442\u043E\u043C), \u0442\u043E - \u043E\u043D\u0430 \u0437\u0430\u043C\u0435\u043D\u0438\u0442\u0441\u044F \u043A\u0430\u0440\u0442\u0438\u043D\u043A\u043E\u0439.\u003Cbr\u003E\n\u041A\u0430\u0440\u0442\u0438\u043D\u043A\u0443 \u043C\u043E\u0436\u043D\u043E \u043C\u0435\u043D\u044F\u0442\u044C \u043F\u043E\u0441\u043B\u0435 \u0432\u0441\u0442\u0430\u0432\u043A\u0438, \u043D\u0430\u0436\u0430\u0432 \u043D\u0430 \u043A\u043B\u0430\u0432\u0438\u0430\u0442\u0443\u0440\u0435 Ctrl \u002B \u0449\u0435\u043B\u043A\u043D\u0443\u0432 \u043F\u043E \u0438\u0437\u043E\u0431\u0440\u0430\u0436\u0435\u043D\u0438\u044E \u041F\u041A\u041C -\u0026gt; \u043F\u043E\u044F\u0432\u0438\u0442\u0441\u044F \u043C\u0435\u043D\u044E \u0022Edit\u0022 (\u043F\u0440\u0430\u0432\u043A\u0430). \u0422\u0430\u043C \u043C\u043E\u0436\u043D\u043E \u0437\u0430\u0434\u0430\u0442\u044C \u0440\u0430\u0437\u043C\u0435\u0440, \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u0438\u0442\u0434.\n\u003C/p\u003E\n\u003Cp\u003E\n\u041A\u0430\u043A-\u0440\u0430\u0437 \u0442\u0430\u043A\u0438 imgur \u0431\u044B\u043B \u0438\u0441\u0445\u043E\u0434\u043D\u043E \u0441\u0434\u0435\u043B\u0430\u043D \u0434\u043B\u044F \u0442\u043E\u0433\u043E, \u0447\u0442\u043E\u0431\u044B \u0445\u043E\u0441\u0442\u0438\u0442\u044C \u043A\u0430\u0440\u0442\u0438\u043D\u043A\u0438 \u0441 \u043D\u0435\u043E\u0433\u0440\u0430\u043D\u0438\u0447\u0435\u043D\u043D\u043E\u0439 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u044C\u044E \u0432\u0441\u0442\u0430\u0432\u043A\u0438 \u043D\u0430 \u0442\u0440\u0435\u0442\u044C\u0438 \u0441\u0430\u0439\u0442\u044B. \u0420\u0435\u0433\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u044F \u0442\u0430\u043C \u043D\u0435 \u0442\u0440\u0435\u0431\u0443\u0435\u0442\u0441\u044F. \u0423 \u0432\u0430\u0441 \u0435\u0441\u0442\u044C \u043A\u0430\u043A\u0430\u044F-\u0442\u043E \u043F\u0440\u0438\u0447\u0438\u043D\u0430 \u043F\u043E \u043A\u043E\u0442\u043E\u0440\u043E\u0439 \u0432\u044B \u043D\u0435 \u043C\u043E\u0436\u0435\u0442\u0435 \u0435\u0433\u043E \u0438\u0441\u043F\u043E\u043B\u04