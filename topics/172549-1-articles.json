/default_k_smiley.gif\u0022 title=\u0022:)\u0022\u003E\u00A0 \u0412 \u044D\u0442\u043E\u043C \u0435\u0441\u0442\u044C \u0431\u043E\u043B\u044C\u0448\u0438\u0435 \u043F\u043B\u044E\u0441\u044B: 1. \u0432\u044B \u0441\u043E\u0445\u0440\u0430\u043D\u044F\u0435\u0442\u0435 \u043A\u043E\u043F\u0438\u0440\u0430\u0439\u0442 \u043D\u0430\u0434 \u0437\u0430\u0433\u0440\u0443\u0436\u0430\u0435\u043C\u044B\u043C. 2. \u0432\u0441\u0435 \u0432\u0430\u0448\u0435 \u0437\u0430\u0433\u0440\u0443\u0436\u0430\u0435\u043C\u043E\u0435 \u043D\u0438\u043A\u0443\u0434\u0430 \u043D\u0435 \u043F\u0440\u043E\u043F\u0430\u0434\u0430\u0435\u0442, \u0435\u0441\u043B\u0438 \u0447\u0442\u043E-\u0442\u043E \u0441\u043B\u0443\u0447\u0430\u0435\u0442\u0441\u044F \u0441 \u0444\u043E\u0440\u0443\u043C\u043E\u043C. 3. \u043D\u0430\u043C \u043D\u0435 \u043D\u0443\u0436\u043D\u043E \u043C\u043E\u0434\u0435\u0440\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u0435\u0449\u0435 \u0438 \u0444\u0443\u043D\u043A\u0446\u0438\u043E\u043D\u0430\u043B\u044C\u043D\u043E\u0441\u0442\u044C CDN - \u0446\u0435\u043D\u0430 \u043D\u0430 \u0438\u0433\u0440\u0443 \u043D\u0435 \u0440\u0430\u0441\u0442\u0435\u0442.\u003Cbr\u003E\n-\n\u003C/p\u003E\n\u003C/div\u003E\n\u003C/blockquote\u003E\n\u003Cp\u003E\n\u0434\u0430 \u043D\u0435 \u0442\u0430\u043A \u044D\u0442\u043E \u0432\u0441\u0435 , \u0443 \u043C\u0435\u043D\u044F \u0435\u0441\u0442\u044C \u043F\u0430\u043F\u043A\u0430 \u043D\u0430 \u043A\u043E\u043C\u043F\u0435 \u0433\u0434\u0435 \u0441\u043E\u0442\u043D\u0438 \u043A\u0430\u0440\u0442\u0438\u043D\u043E\u043A \u0438 \u0447\u0442\u043E \u0431\u044B \u043C\u043D\u0435 \u0441\u044E\u0434\u0430 \u0438\u0445 \u0432\u0441\u0442\u0430\u0432\u0438\u0442\u044C \u044F \u0434\u043E\u043B\u0436\u0435\u043D \u043A\u0443\u0434\u0430 \u0442\u043E \u0438\u0445 \u0437\u0430\u043B\u0438\u0442\u044C, \u0430 \u043F\u043E\u0442\u043E\u043C \u0434\u0430\u0432\u0430\u0442\u044C \u0441\u0441\u044B\u043B\u043A\u0443, \u0432\u0441\u0435 \u043F\u043E\u043D\u044F\u0442\u043D\u043E \u043A\u0440\u043E\u043C\u0435 \u043E\u0434\u043D\u043E\u0433\u043E- \u0430 \u043C\u043D\u0435 \u0442\u043E \u0437\u0430\u0447\u0435\u043C \u0442\u0430\u043A\u0438\u0435 \u0442\u0430\u043D\u0446\u044B?\n\u003C/p\u003E\n\u003Cp\u003E\n\u043D\u0443 \u0435\u0441\u0442\u044C \u0433\u0434\u0435 \u0442\u043E \u0443 \u043C\u0435\u043D\u044F \u0437\u0430\u043B\u0438\u0442\u044B\u0435 \u043A\u0430\u0440\u0442\u0438\u043D\u043A\u0438 \u0432 4 \u041A \u0444\u043E\u0440\u043C\u0430\u0442\u0435\n\u003C/p\u003E\n\u003Cp\u003E\n\u003Ca href=\u0022http://uploads.ru/aApcr.jpg\u0022 rel=\u0022external nofollow\u0022\u003EWovKino\u003C/a\u003E\n\u003C/p\u003E\n\u003Cp\u003E\n\u0432\u043E\u0442 \u043F\u043E\u043B\u0435\u0437 \u0432\u0441\u0442\u0430\u0432\u0438\u043B \u0441\u0441\u044B\u043B\u043A\u0443, \u043D\u043E \u0442\u0443\u0442 \u0442\u043E \u0432\u0441\u0435 \u0440\u0430\u0432\u043D\u043E \u043D\u0435\u0442 \u043A\u0430\u0440\u0442\u0438\u043A\u0438 \u0438 \u043A\u0430\u043A\u043E\u0439 \u0441\u043C\u044B\u0441\u043B \u043C\u043D\u0435 \u0442\u0430\u043A\u0438\u0435 \u0442\u0430\u043D\u0446\u044B \u0437\u0430\u0436\u0438\u0433\u0430\u0442\u044C?\n\u003C/p\u003E\n\u003Cp\u003E\n\u003Ca href=\u0022http://uploads.ru/aApcr.jpg\u0022 rel=\u0022external nofollow\u0022\u003E\u003Cimg src=\u0022http://s9.uploads.ru/t/aApcr.jpg\u0022 class=\u0022ipsImage\u0022 alt=\u0022aApcr.jpg\u0022\u003E\u003C/a\u003E\n\u003C/p\u003E\n\u003Cp\u003E\n\u044D\u0442\u0443\u00A0 \u0441\u0442\u0440\u043E\u0447\u043A\u0443 \u0432\u0441\u0442\u0430\u0432\u043B\u044F\u043B 5 \u043C\u0438\u043D\u0443\u0442 \u043A\u0440\u0443\u0442\u0438\u043B\u043E\u0441\u044C , \u043D\u043E \u0442\u0430\u043A \u0438 \u043D\u0435 \u0432\u0441\u0442\u0430\u0432\u0438\u043B\u043E\u0441\u044C \u0438 \u0432\u043E\u0442 \u0432\u043E\u043F\u0440\u043E\u0441: \u044F \u0447\u0442\u043E \u0442\u0430\u043A \u0431\u0443\u0434\u0443 \u043F\u0435\u0440\u0435\u0436\u0438\u0432\u0430\u0442\u044C \u0438 \u044