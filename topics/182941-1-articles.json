\u043D\u043E\u0433\u043E \u0443\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0438\u044F \u043D\u0443\u0436\u0435\u043D \u0441\u0438\u0433\u043D\u0430\u043B, \u0442\u043E \u043F\u0440\u0438\u0434\u0435\u0442\u0441\u044F \u0432\u0435\u0437\u0434\u0435 \u0432\u0435\u0441\u0438\u0442\u044C \u043A\u043E\u043C\u043C\u0443\u043D\u0438\u043A\u0430\u0446\u0438\u043E\u043D\u043D\u044B\u0435 \u0441\u0435\u0442\u0438. \u0418\u043D\u0430\u0447\u0435 \u0432\u0441\u0442\u0440\u044F\u043D\u0435\u0448\u044C \u043A\u0430\u043A \u043D\u0430\u043F\u0440\u0438\u043C\u0435\u0440 \u044F: \u0435\u0441\u0442\u044C \u0440\u043E\u0432\u0435\u0440 \u043D\u0430 \u0414\u044E\u043D\u0435, \u043D\u043E \u0442\u0430\u0440\u0435\u043B\u043A\u0430 \u0443 \u043D\u0435\u0433\u043E \u043C\u0430\u043B\u0435\u043D\u044C\u043A\u0430\u044F (\u0431\u043E\u043B\u044C\u0448\u0443\u044E \u043D\u0435\u043A\u0443\u0434\u0430 \u043F\u043E\u0441\u0442\u0430\u0432\u0438\u0442\u044C), \u0430 \u0440\u0435\u0442\u0440\u0430\u043D\u0441\u043B\u044F\u0442\u043E\u0440 \u043F\u0440\u043E\u0445\u043E\u0434\u0438\u0442 \u043D\u0430\u0434 \u0440\u043E\u0432\u0435\u0440\u043E\u043C \u043D\u0435\u0447\u0430\u0441\u0442\u043E \u0438 \u0434\u0430\u0436\u0435 \u0443 \u043D\u0435\u0433\u043E \u0443\u0440\u043E\u0432\u0435\u043D\u044C \u0441\u0438\u0433\u043D\u0430\u043B\u0430 \u043D\u0435\u0432\u044B\u0441\u043E\u043A\u0438\u0439. \u0412 \u0438\u0442\u043E\u0433\u0435 \u0440\u043E\u0432\u0435\u0440 \u0435\u0441\u0442\u044C, \u0430 \u0442\u043E\u043B\u043A\u0443 \u043D\u043E\u043B\u044C, \u0443\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0438\u0435 \u0437\u0430\u0431\u043B\u043E\u043A\u0438\u0440\u043E\u0432\u0430\u043D\u043E. \u041D\u0443\u0436\u043D\u043E \u0441\u0435\u0439\u0447\u0430\u0441 \u0442\u0430\u0449\u0438\u0442\u044C \u043D\u0430 \u0414\u044E\u043D\u0443 \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u0438 \u0441\u0432\u044F\u0437\u0438 \u0441 \u043C\u0430\u043A\u0441\u0438\u043C\u0430\u043B\u044C\u043D\u043E \u043C\u043E\u0449\u043D\u044B\u043C\u0438 \u0442\u0430\u0440\u0435\u043B\u043A\u0430\u043C\u0438.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0421\u043E ScanSat \u0432\u0441\u0435 \u043D\u0435\u043C\u043D\u043E\u0433\u043E \u0438\u043D\u0430\u0447\u0435. \u041E\u043D \u0434\u0430\u0435\u0442 \u043E\u0447\u0435\u043D\u044C \u043F\u043E\u0434\u0440\u043E\u0431\u043D\u044B\u0435 \u043A\u0430\u0440\u0442\u044B \u043F\u043B\u0430\u043D\u0435\u0442. \u041C\u043E\u0436\u043D\u043E \u0432\u044B\u0431\u0438\u0440\u0430\u0442\u044C \u043E\u0447\u0435\u043D\u044C\u00A0 \u0442\u043E\u0447\u043D\u043E \u043C\u0435\u0441\u0442\u0430 \u043F\u043E\u0441\u0430\u0434\u043E\u043A, \u0432\u0438\u0434\u0435\u0442\u044C \u0432\u044B\u0441\u043E\u0442\u0443 \u0432 \u044D\u0442\u043E\u043C \u043C\u0435\u0441\u0442\u0435, \u0443\u043A\u043B\u043E\u043D \u0438 \u0438\u043C\u0435\u0442\u044C \u0442\u043E\u0447\u043D\u044B\u0435 \u043A\u043E\u043E\u0440\u0434\u0438\u043D\u0430\u0442\u044B. \u0415\u0441\u043B\u0438 \u0438\u0433\u0440\u0430\u0442\u044C \u0432 \u043A\u0430\u0440\u044C\u0435\u0440\u0443 \u0438 \u0438\u043C\u0435\u0442\u044C \u043A\u043E\u043D\u0442\u0440\u0430\u043A\u0442\u044B \u043D\u0430 \u0432\u044B\u043F\u043E\u043B\u043D\u0435\u043D\u0438\u0435 \u0438\u0441\u0441\u043B\u0435\u0434\u043E\u0432\u0430\u043D\u0438\u0439 \u0432 \u043A\u043E\u043D\u043A\u0440\u0435\u0442\u043D\u044B\u0445 \u0442\u043E\u0447\u043A\u0430\u0445, \u0442\u0430\u043A\u0438\u0435 \u043A\u0430\u0440\u0442\u044B \u0441\u0430\u043C\u043E\u0435 \u043E\u043D\u043E. \u042F \u0431\u0435\u0437 \u043D\u0438\u0445 \u0432\u043E\u043E\u0431\u0449\u0435 \u043D\u0435 \u043F\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043B\u044F\u044E \u043F\u043E\u043B\u0435\u0442\u044B \u043D\u0430 \u0434\u0440\u0443\u0433\u0438\u0435 \u043F\u043B\u0430\u043D\u0435\u0442\u044B. \u041B\u0435\u0442\u0435\u0442\u044C \u043A\u0443\u0434\u0430-\u043D\u0438\u0431\u0443\u0434\u044C \u043F\u043E \u043F\u0440\u0438\u043D\u0446\u0438\u043F\u0443 \u0433\u043B\u0430\u0432\u043D\u043E\u0435 \u0447\u0442\u043E \u043F\u043E\u043F\u0430\u043B\u0438, \u043D\u0435 \u0434\u043B\u044F \u043C\u0435\u043D\u044F. \u041A\u0435\u0440\u0431\u043D\u0435\u0442 \u0434\u0430\u0435\u0442 \u043A\u0430\u043A\u0438\u0435-\u0442\u043E \u043F\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043B\u0435\u043D\u0438\u044F, \u043D\u043E \u0438\u043D\u0444\u043E\u0440\u043C\u0430\u0442\u0438\u0432\u043D\u043E\u0441\u0442\u044C \u0443 \u043D\u0435\u0433\u043E \u043D\u0430\u043C\u043D\u043E\u0433\u043E \u043D\u0438\u0436\u0435 \u0438 \u044F \u043D\u0430\u043F\u0440\u0438\u043C\u0435\u0440 \u0438\u043C \u043F\u043E\u0447\u0442\u0438 \u043D\u0435 \u043F\u043E\u043B\u044C\u0437\u0443\u044E\u0441\u044C.\n\u003C/p\u003E\n"},{"CreatedByName":"rebel-1","CreatedById":170896,"CreatedDateTime":"2019-03-19T17:17:41Z","Content":"\n\u003Cp\u003E\n\u041D\u0443 \u0445\u0437, \u043F\u043E\u043F\u0440\u043E\u0431\u043E\u0432\u0430\u043B \u0432 \u043F\u0435\u0441\u043E\u0447\u043A\u0435 \u0431\u0435\u0437 RT... \u041A\u0430\u043D\u0430\u043B\u044B \u0441\u0432\u044F\u0437\u0438 \u043F\u0440\u0438\u043D\u0446\u0438\u043F \u0442\u0430\u043A\u043E\u0439 \u0436\u0435, \u043E\u0442\u043B\u0438\u0447\u0438\u0435 \u0442\u043E\u043B\u044C\u043A\u043E \u043F\u043E \u043C\u043E\u0449\u043D\u043E\u0441\u0442\u0438 \u0441\u0438\u0433\u043D\u0430\u043B\u0430(\u0443 \u0441\u0442\u043E\u043A\u0430), \u0430 \u0443 RT \u0434\u0430\u043B\u044C\u043D\u043E\u0441\u0442\u044C \u0430\u043D\u0442\u0435\u043D\u043D\u044B. \u0415\u0449\u0435 \u0443 RT \u0435\u0441\u0442\u044C \u0431\u043E\u0440\u0442\u0430\u0447, \u043A\u043E\u0442\u043E\u0440\u044B\u043C \u044F \u0432 \u043F\u0440\u0438\u043D\u0446\u0438\u043F\u0435 \u043D\u0435 \u043F\u043E\u043B\u044C\u0437\u0443\u044E\u0441\u044C. \u0418 \u0434\u0430, \u0443 RT \u0430\u043D\u0442\u0435\u043D\u043D\u044B \u043D\u0430\u0434\u043E \u043D\u0430\u043F\u0440\u0430\u0432\u043B\u044F\u0442\u044C \u0432\u0440\u0443\u0447\u043D\u0443\u044E, \u0438 \u0443 \u043D\u0438\u0445 \u0435\u0441\u0442\u044C \u0448\u0438\u0440\u0438\u043D\u0430 \u043B\u0443\u0447\u0430. \u0420\u0435\u0448\u0438\u043B \u043E\u0441\u0442\u0430\u0432\u0438\u0442\u044C RT, \u043E\u043D \u043A\u0430\u043A \u0442\u043E \u043F\u043E\u0445\u0430\u0440\u0434\u043A\u043E\u0440\u043D\u0435\u0439 \u0438 \u043F\u043E\u043D\u044F\u0442\u043D\u0435\u0439, \u043D\u043E \u0432\u043E\u0442 \u0441\u0442\u043E\u043A\u043E\u0432\u0430\u044F \u0444\u0438\u0448\u043A\u0430 CN \u0441 \u043C\u043E\u0449\u043D\u043E\u0441\u0442\u044C\u044E \u0441\u0438\u0433\u043D\u0430\u043B\u0430, \u0432\u043B\u0438\u044F\u044E\u0449\u0430\u044F \u043D\u0430 \u043A\u043E\u043D\u0442\u0440\u043E\u043B\u044C \u0438 \u043F\u0435\u0440\u0435\u0434\u0430\u0447\u0443,\u00A0\u043A\u043E\u043D\u0435\u0447\u043D\u043E \u043E\u0433\u043E\u043D\u044C.\n\u003C/p\u003E\n\u003Cp\u003E\n\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u0410 \u0441\u043E \u0441\u043A\u0430\u043D\u0441\u0430\u0442\u043E\u043C \u0442\u043E \u044F \u0447\u0435 \u0437\u0430\u0432\u0435\u043B\u0441\u044F \u0442\u043E. \u041D\u0443 \u0432\u043E \u043F\u0435\u0440\u0432\u044B\u0445 \u0440\u0435\u0430\u043B\u044C\u043D\u043E \u0433\u0435\u043C\u043E\u0440 \u0438\u043C\u0435\u0442\u044C \u043A\u0443\u0447\u0443 \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u043E\u0432, \u0432\u043E \u0432\u0442\u043E\u0440\u044B\u0445 \u0432\u043E\u0442 \u044D\u0442\u043E \u0432\u043E\u0442 \u0432\u044B\u0441\u043E\u043A\u043E\u0442\u043E\u0447\u043D\u043E\u0435 \u0441\u043A\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u0435 \u0441\u043B\u0438\u0448\u043A\u043E\u043C \u0443\u0436 \u0434\u043E\u043B\u0433\u043E\u0435. \u0418 \u0442\u0440\u0435\u0442\u044C\u0435 - \u043D\u0430 \u043E\u0440\u0431\u0438\u0442\u0435 \u043A\u043E\u0433\u0434\u0430 \u043D\u0430 \u043B\u0443\u043D\u0430\u0445 \u043F\u043E\u0432\u0435\u0441\u0438\u043B \u043A\u0443\u0447\u0443 \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u043E\u0432 \u0421\u043A\u0430\u043D\u0441\u0430\u0442\u0430 - \u0438\u0433\u0440\u0430 \u043D\u0430\u0447\u0430\u043B\u0430 \u0434\u0438\u043A\u043E \u0444\u0440\u0438\u0437\u0438\u0442\u044C. \u041A\u043E\u043C\u043F \u043D\u0435 \u0441\u043B\u0430\u0431\u0435\u043D\u044C\u043A\u0438\u0439, \u0435\u0441\u043B\u0438 \u0447\u0442\u043E. R5 2600X, \u043C\u0430\u043C\u043A\u0430 \u043D\u0430 \u0425470, 16 \u0433\u0438\u0433\u043E\u0432 \u0442\u043E\u043F\u043E\u0432\u043E\u0439 \u0413.\u0421\u043A\u0438\u043B\u043B \u0424\u043B\u0430\u0440\u0435-\u0425, \u0413\u0422\u04251070. \u0422\u0410\u043A \u0447\u0442\u043E \u0445\u0437, \u0442\u043E \u043B\u0438 \u043E\u0442 \u0441\u043A\u0430\u043D\u0441\u0430\u0442\u0430 \u0444\u0440\u0438\u0437\u0438\u0442, \u0442\u043E \u043B\u0438 RT, \u0430 \u043C\u043E\u0436\u0435\u0442 \u0441\u0430\u043C\u0430 \u0438\u0433\u0440\u0430. \u041D\u043E \u0434\u0438\u043A\u043E \u0431\u0435\u0441\u0438\u0442. \u041D\u0438\u043A\u0430\u043A\u0438\u0445 \u0441\u0442\u0430\u043D\u0446\u0438\u0439, \u0431\u0430\u0437 \u0438 \u043F\u0440\u043E\u0447\u0438\u0445 \u043D\u0438\u0448\u0442\u044F\u043A\u043E\u0432 \u0432\u043E\u043E\u0431\u0449\u0435 \u043D\u0435\u0442, \u043D\u0430\u0447\u0430\u043B\u044C\u043D\u044B\u0439 \u044D\u0442\u0430\u043F \u043A\u0430\u0440\u044C\u0435\u0440\u044B,\u00A0\u0442\u043E\u043B\u044C\u043A\u043E \u043F\u043E 4 \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u0430 \u043D\u0430 \u043B\u0443\u043D\u0430\u0445 \u0438 \u043D\u0430 \u0441\u0430\u043C\u043E\u043C \u041A\u0435\u0440\u0431\u0438\u043D\u0435. \u041C\u043E\u0436\u0435\u0442 \u043A\u0442\u043E \u0441\u0442\u0430\u043B\u043A\u0438\u0432\u0430\u043B\u0441\u044F?\n\u003C/p\u003E\n"},{"CreatedByName":"Sokol_323","CreatedById":185655,"CreatedDateTime":"2019-03-21T12:32:46Z","Content":"\n\u003Cp\u003E\n\u041D\u0430\u0441\u0447\u0435\u0442 ScanSat \u0438 \u0434\u043E\u043B\u0433\u043E \u0438 \u0434\u043E\u0440\u043E\u0433\u043E \u043D\u0435 \u0441\u043E\u0433\u043B\u0430\u0441\u0435\u043D. \u0420\u0435\u0430\u043B\u044C\u043D\u043E \u043D\u0443\u0436\u043D\u043E \u043C\u0430\u043A\u0441\u0438\u043C\u0443\u043C 3 \u0441\u043F\u0443\u0442\u043D\u0438\u043A\u0430 \u0441\u043E \u0441\u043A\u0430\u043D\u0435\u0440\u0430\u043C\u0438 \u0438 \u0434\u0435\u043B\u044C\u0442\u043E\u0439, \u0447\u0442\u043E\u0431\u044B \u043F\u0435\u0440\u0435\u0432\u043E\u0434\u0438\u0442\u044C \u0438\u0445 \u0441 \u043E\u0440\u0431\u0438\u0442\u044B \u043D\u0430 \u043E\u0440\u0431\u0438\u0442\u0443. \u042D\u0442\u043E \u0435\u0441\u043B\u0438 \u0447\u0442\u043E-\u0442\u043E \u0442\u0438\u043F\u0430 \u041A\u0435\u0440\u0431\u0438\u043D\u0430. \u041F\u0440\u0438 \u044D\u0442\u043E\u043C \u043E\u043D\u0438 \u0441\u043F\u043E\u043A\u043E\u0439\u043D\u043E \u043C\u043E\u0433\u0443\u0442 \u0432\u044B\u043F\u043E\u043B\u043D\u044F\u0442\u044C \u0435\u0449\u0435 \u0438 \u0440\u043E\u043B\u044C \u0440\u0435\u0442\u0440\u0430\u043D\u0441\u043B\u044F\u0442\u043E\u0440\u043E\u0432 \u0438\u043B\u0438 \u043D\u043E\u0441\u0438\u0442\u0435\u043B\u0435\u0439 \u0438\u043D\u044B\u0445 \u043F\u0440\u0438\u0431\u043E\u0440\u043E\u0432. \u042F\u00A0\u043E\u0431\u044B\u0447\u043D\u043E \u0438\u0445 \u0434\u0435\u043B\u0430\u044E \u043D\u0430 \u0431\u0430\u0437\u0435 \u0431\u043E\u043B\u044C\u0448\u043E\u0433\u043E \u0448\u0435\u0441\u0442\u0438\u0433\u0440\u0430\u043D\u043D\u043E\u0433\u043E \u043A\u043E\u043C\u0430\u043D\u0434\u043D\u043E\u0433\u043E \u043C\u043E\u0434\u0443\u043B\u044F, \u0435\u0441\u043B\u0438 \u043D\u0435 \u0434\u043E\u0441\u0442\u0443\u043F\u0435\u043D, \u0442\u043E \u043F\u043E\u0434\u043E\u0439\u0434\u0435\u0442 \u043B\u044E\u0431\u043E\u0439. \u0414\u043E\u0431\u0430\u0432\u043B\u044F\u0435\u043C \u043F\u0430\u043D\u0435\u043B\u0438, \u0431\u0430\u0442\u0430\u0440\u0435\u0439\u043A\u0438 (\u0441\u043A\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u0435 \u0436\u0440\u0435\u0442 \u043C\u043D\u043E\u0433\u043E \u044D\u043B\u0435\u043A\u0442\u0440\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u0438 \u0447\u0442\u043E\u0431\u044B \u043D\u0435 \u0442\u0443\u0445 \u043D\u0430 \u0442\u0435\u043C\u043D\u043E\u0439 \u0441\u0442\u043E\u0440\u043E\u043D\u0435, \u043C\u043E\u0436\u043D\u043E \u0435\u0449\u0435 \u0438 \u0420\u0418\u0422\u042D\u0413, \u043D\u043E \u0432 \u0440\u0430\u043D\u043D\u0435\u0439 \u043A\u0430\u0440\u044C\u0435\u0440\u0435 \u0435\u0433\u043E \u0441\u043A\u043E\u0440\u0435\u0435 \u0432\u0441\u0435\u0433\u043E \u043D\u0435\u0442).\u00A0\u00A0\u0414\u0430\u043B\u0435\u0435 \u0440\u0430\u0437\u0432\u0435\u0448\u0438\u0432\u0430\u0435\u043C \u0442\u0430\u043A \u043F\u043E \u043D\u0430\u043A\u043B\u043E\u043D\u0435\u043D\u0438\u044E 15-30 \u0433\u0440, 45-60 \u0438 \u043E\u043A\u043E\u043B\u043E 90 \u0433\u0440. \u0412\u044B\u0441\u043E\u0442\u0430 \u0434\u043B\u044F \u043D\u0438\u0437\u043A\u043E\u0439 \u0430\u043B\u044C\u0442\u0438\u043C\u0435\u0442\u0440\u0438\u0438 \u0438 \u043C\u0443\u043B\u044C\u0442\u0438\u0441\u043A\u0430\u043D\u0430 (\u0431\u0438\u043E\u043C\u044B, \u0430\u043D\u043E\u043C\u0430\u043B\u0438\u0438) 250 \u043A\u043C-500 \u043A\u043C, \u0432\u044B\u0441\u043E\u043A\u0430\u044F \u0430\u043B\u044C\u0442\u0438\u043C\u0435\u0442\u0440\u0438\u044F 500-750 \u043A\u043C. \u0412 \u043A\u0430\u0440\u044C\u0435\u0440\u0435 \u0435\u0449\u0435 \u0434\u0430\u044E\u0442 \u043A\u043E\u043D\u0442\u0440\u0430\u043A\u0442\u044B \u043D\u0430 \u0441\u043A\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u0435 \u0438 \u043E\u043D\u0438 \u0432 \u043F\u0440\u0438\u043D\u0446\u0438\u043F\u0435 \u0434\u043E\u0432\u043E\u043B\u044C\u043D\u043E \u0434\u0435\u043D\u0435\u0436\u043D\u044B\u0435. \u041C\u0435\u043D\u044F \u0432 \u043F\u0435\u0440\u0432\u0443\u044E \u043E\u0447\u0435\u0440\u0435\u0434\u044C \u0432\u0441\u0435\u0433\u0434\u0430 \u0438\u043D\u0442\u0435\u0440\u0435\u0441\u0443\u044E\u0442 \u0431\u0438\u043E\u043C\u044B, \u043F\u043E\u044D\u0442\u043E\u043C\u0443 \u0432\u0435\u0448\u0430\u044E \u043D\u0430 \u043A\u0430\u0436\u0434\u044B\u0439 \u0441\u043F\u0443\u0442\u043D\u0438\u043A \u043C\u0443\u043B\u044C\u0442\u0438\u0441\u043A\u0430\u043D. \u0427\u0435\u0440\u0435\u0437 \u043A\u0430\u043A\u043E\u0435-\u0442\u043E \u0432\u0440\u0435\u043C\u044F \u043C\u0435\u043D\u044F\u044E \u043D\u0430\u043A\u043B\u043E\u043D\u0435\u043D\u0438\u0435 \u043E\u0440\u0431\u0438\u0442. \u0412 \u0438\u0434\u0435\u0430\u043B\u0435 \u043A\u043E\u043D\u0435\u0447\u043D\u043E \u043F\u0440\u043E\u0439\u0442\u0438 \u043E\u0442 0 \u0434\u043E 180 \u0433\u0440 \u0441 \u0448\u0430\u0433\u043E\u043C 30 \u0433\u0440, \u043D\u043E \u044D\u0442\u043E \u043D\u0435 \u0440\u0435\u0430\u043B\u044C\u043D\u043E, \u0442.\u043A. \u0437\u0430\u0442\u0440\u0430\u0442\u043D\u043E \u043F\u043E \u0434\u0435\u043B\u044C\u0442\u0435, \u043F\u043E\u044D\u0442\u043E\u043C\u0443 \u043A\u0430\u043A \u043F\u043E\u043B\u0443\u0447\u0438\u0442\u0441\u044F. \u0418\u0442\u043E\u0433\u043E \u043D\u0430 \u041A\u0435\u0440\u0431\u0438\u043D \u0443\u0445\u043E\u0434\u0438\u0442 \u043F\u043E\u0440\u044F\u0434\u043A\u0435 20-30 \u0434\u043D\u0435\u0439 (\u0442\u043E\u0447\u043D\u043E \u043D\u0435 \u043F\u043E\u043C\u043D\u044E). \u041B\u0443\u043D\u044B \u043E\u0442\u0441\u043A\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u043B \u0435\u0449\u0435 \u0431\u044B\u0441\u0442\u0440\u0435\u0435, \u043F\u0440\u0438\u0447\u0435\u043C \u043D\u0430 \u041C\u0438\u043D\u043C\u0443\u0441 \u0432\u0435\u0441\u0438\u043B \u0432\u043E\u043E\u0431\u0449\u0435 \u043E\u0434\u0438\u043D \u0441\u043A\u0430\u043D\u0435\u0440 \u0441 \u0445\u043E\u0440\u043E\u0448\u0435\u0439 \u0434\u0435\u043B\u044C\u0442\u043E\u0439 \u0438 \u0433\u043E\u043D\u044F\u043B \u0435\u0433\u043E \u043F\u043E \u043A\u0440\u0443\u0433\u0443. \u0414\u044E\u043D\u0430 \u0438 \u0418\u0432 \u043E\u0442\u0441\u043A\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u043B\u0438\u0441\u044C \u0434\u043E\u0432\u043E\u043B\u044C\u043D\u043E\u00A0\u0431\u044B\u0441\u0442\u0440\u043E, \u043D\u043E \u043D\u0430 \u0414\u044E\u043D\u0435 \u043B\u043E\u0445\u0430\u043D\u0443\u043B\u0441\u044F \u0441 \u043C\u043E\u0449\u043D\u043E\u0441\u0442\u044C\u044E \u0430\u043D\u0442\u0435\u043D\u043D (\u043A\u0430\u0440\u0442\u0430-\u0442\u043E \u0435\u0441\u0442\u044C, \u0441\u0430\u0434\u0438\u0442\u0441\u044F \u043C\u043E\u0436\u043D\u043E, \u043D\u043E \u0441\u0438\u0433\u043D\u0430\u043B \u043D\u0430 \u043F\u043E\u0432\u0435\u0440\u0445\u043D\u043E\u0441\u0442\u044C \u043D\u0435 \u043F\u0440\u043E\u0445\u043E\u0434\u0438\u0442, \u0442\u043E\u043B\u044C\u043A\u043E \u0435\u0441\u043B\u0438 \u043B\u0435\u0442\u0430\u0442\u044C \u043F\u0440\u044F\u043C\u043E \u043D\u0430\u0434 \u0442\u043E\u0447\u043A\u043E\u0439 \u043F\u043E\u0441\u0430\u0434\u043A\u0438 \u0438 \u0447\u0442\u043E\u0431\u044B \u041A\u0435\u0440\u0431\u0438\u043D \u0431\u044B\u043B \u043D\u0430 \u043F\u0440\u044F\u043C\u043E\u0439).\u00A0 \u0412\u043E\u0442 \u0441\u0438\u0436\u0443 \u0438 \u0434\u0443\u043C\u0430\u044E \u043A\u0430\u043A \u0442\u0430\u043C \u043F\u043E\u0441\u0442\u0440\u043E\u0438\u0442\u044C\u00A0\u0445\u043E\u0440\u043E\u0448\u0443\u044E \u0441\u0435\u0442\u044C, \u043D\u0435 \u0445\u043E\u0447\u0435\u0442\u0441\u044F \u0436\u0434\u0430\u0442\u044C \u043F\u043E\u0447\u0442\u0438 2 \u0433\u043E\u0434\u0430. \u041D\u0430 \u0418\u0432\u00A0\u0443 \u043C\u0435\u043D\u044F \u0432\u044B\u0448\u043B\u043E \u0447\u0443\u0442\u044C \u043F\u043E\u043B\u0443\u0447\u0448\u0435, \u0438\u0441\u043F\u0440\u0430\u0432\u0438\u043B \u043D\u0435\u0434\u043E\u0441\u0442\u0430\u0442\u043A\u0438 \u043F\u043E\u0441\u0430\u0434\u043E\u0447\u043D\u044B\u0445 \u0438 \u043E\u0440\u0431\u0438\u0442\u0430\u043B\u044C\u043D\u044B\u0445 \u0430\u043F\u043F\u0430\u0440\u0430\u0442\u043E\u0432, \u043F\u043E\u044D\u0442\u043E\u043C\u0443 \u0442\u0430\u043C \u0431\u043E\u043B\u0435\u0435 \u043C\u0435\u043D\u0435\u0435 \u0441\u043E \u0441\u0432\u044F\u0437\u044C\u044E. \u0418 \u0441\u043A\u0430\u043D\u044B \u043D\u0435\u043F\u043B\u043E\u0445\u0438\u0435. \u041D\u043E \u0432\u043E\u0442 \u043D\u0430 \u0413\u0438\u043B\u0438 \u0443\u0436\u0435 \u043D\u0435 \u0445\u0432\u0430\u0442\u0438\u043B\u043E, \u0445\u043E\u0442\u044F \u0410\u0439\u043A \u043D\u0430\u043F\u0440\u0438\u043C\u0435\u0440 \u0441\u0443\u043C\u0435\u043B \u043E\u0442\u0441\u043A\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u043E\u0434\u043D\u0438\u043C \u0430\u043F\u043F\u0430\u0440\u0430\u0442\u043E\u043C \u0441 \u0414\u044E\u043D\u044B. \u0418 \u0441\u0435\u0439\u0447\u0430\u0441 \u043F\u0440\u0438\u043B\u0435\u0442\u0435\u0432\u0448\u0438\u0439 \u0442\u0443\u0434\u0430 \u0441\u043F\u0435\u0446\u0438\u0430\u043B\u044C\u043D\u043E \u0441\u043A\u0430\u043D\u0435\u0440 \u0442\u0443\u043F\u043E \u0440\u0430\u0431\u043E\u0442\u0430\u0435\u0442 \u0440\u0435\u0442\u0440\u0430\u043D\u0441\u043B\u044F\u0442\u043E\u0440\u043E\u043C.\n\u003C/p\u003E\n\u003Cp\u003E\n\u041D\u0435\u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043C\u043E\u0434\u044B \u043C\u043E\u0433\u0443\u0442 \u0434\u0430\u0432\u0430\u0442\u044C\u00A0\u0444\u0440\u0438\u0437\u044B. ScanSat, \u043A\u043E\u0433\u0434\u0430 \u0440\u0430\u0431\u043E\u0442\u0430\u0435\u0442 \u0432 \u0444\u043E\u043D\u0435 \u043D\u0435 \u0437\u0430\u043C\u0435\u0442\u0435\u043D, \u0430 \u0432\u043E\u0442 \u0435\u0441\u043B\u0438 \u043E\u0442\u043A\u0440\u044B\u0442\u044C \u043A\u0430\u0440\u0442\u0443 \u0438 \u0447\u0442\u043E-\u043D\u0438\u0431\u0443\u0434\u044C \u043F\u043B\u0430\u043D\u0438\u0440\u043E\u0432\u0430\u0442\u044C,\u00A0 \u043C\u043E\u0436\u0435\u0442 \u0442\u043E\u0440\u043C\u043E\u0437\u043D\u0443\u0442\u044C.\u00A0\u041C\u043E\u0433\u0443\u0442 \u0431\u044B\u0442\u044C \u043A\u043E\u043D\u0444\u043B\u0438\u043A\u0442\u044B. \u042F \u0438\u043C\u0435\u043B 82 \u043C\u043E\u0434\u0430. \u0421\u0435\u0439\u0447\u0430\u0441 75. \u0418 \u044D\u0442\u043E \u0441 \u041A\u043E\u043F\u0435\u0440\u043D\u0438\u043A\u043E\u043C \u0438 \u041E\u0420\u041C, \u0430\u0442\u043C\u043E\u0441\u0444\u0435\u0440\u043E\u0439 (\u043E\u043D\u0438 \u0441\u0438\u043B\u044C\u043D\u043E \u0433\u0440\u0443\u0437\u044F\u0442 \u0441\u0438\u0441\u0442\u0435\u043C\u0443). \u0420\u0430\u0431\u043E\u0442\u0430\u0435\u0442 \u043F\u043E\u043A\u0430 \u0440\u043E\u0432\u043D\u043E \u0438 \u043F\u0435\u0440\u0435\u0441\u0442\u0430\u043B\u043E \u0436\u0440\u0430\u0442\u044C \u043E\u043F\u0435\u0440\u0430\u0442\u0438\u0432\u043A\u0443. \u041D\u043E \u0442\u0430\u043A \u0438 \u043D\u0435 \u043F\u043E\u043D\u044F\u043B, \u043A\u0442\u043E \u0435\u0435 \u0433\u0440\u0443\u0437\u0438\u043B, \u0447\u0442\u043E \u0438\u043D\u043E\u0433\u0434\u0430 \u043D\u0435 \u0445\u0432\u0430\u0442\u0430\u043B\u043E 16 \u0413\u0438\u0433\u043E\u0432 \u0438 \u0432\u044B\u043B\u0435\u0442\u0430\u043B \u0441 \u043F\u0435\u0440\u0435\u0433\u0440\u0443\u0437\u043E\u043C \u043F\u0430\u043C\u044F\u0442\u0438 \u043D\u0430 80-85 \u043F\u0440\u043E\u0446\u0435\u043D\u0442\u043E\u0432 (\u0441\u0435\u0439\u0447\u0430\u0441 \u043C\u0430\u043A\u0441\u0438\u043C\u0443\u043C 65 \u0432\u0438\u0434\u0435\u043B). \u0415\u0441\u043B\u0438 \u0443\u0431\u0440\u0430\u0442\u044C \u041A\u041A \u0438 \u0441\u0431\u043E\u0440\u043A\u0443 \u0438\u0437 \u0432\u0441\u044F\u043A\u0438\u0445 \u0443\u043A\u0440\u0430\u0448\u0430\u0442\u0435\u043B\u044C\u0441\u0442\u0432\u00A0(\u043A\u043E\u043C\u043F\u0438\u043B\u044F\u0446\u0438\u044F \u0438\u0437 \u0434\u0432\u0443\u0445 \u041A\u0435\u0440\u0431\u0438\u043D \u0421\u0430\u0439\u0434\u043E\u0432 \u0438 \u041A\u0435\u0440\u0431\u0438\u043D \u0421\u0438\u0442\u0438), \u0442\u043E \u043D\u0430\u0432\u0435\u0440\u043D\u043E\u0435 \u0431\u0443\u0434\u0435\u0442 \u0435\u0449\u0435 \u043B\u0443\u0447\u0448\u0435. \u041D\u043E \u044D\u0442\u0438 \u043C\u043E\u0434\u044B \u0443\u0431\u0438\u0440\u0430\u0442\u044C \u043D\u0435 \u0445\u043E\u0447\u0443, \u0445\u043E\u0442\u044F \u043E\u043D\u0438 \u0442\u044F\u0436\u0435\u043B\u044B\u0435, \u043D\u0435 \u043B\u0435\u0433\u0447\u0435 \u0430\u0442\u043C\u043E\u0441\u0444\u0435\u0440\u044B \u0438 \u043F\u0440.\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u0412\u043E\u0442. \u0410 \u0442\u0430\u043A \u0431\u044B \u0435\u0449\u0435 \u043F\u0440\u043E\u0431\u0435\u0436\u0430\u0442\u044C\u0441\u044F \u043F\u043E \u043C\u043E\u0434\u0430\u043C, \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C \u0444\u043E\u0440\u0443\u043C. \u041D\u0430\u0440\u043E\u0434 \u0438\u043D\u043E\u0433\u0434\u0430 \u043F\u0438\u0448\u0435\u0442, \u043A\u0430\u043A\u0438\u0435 \u043C\u043E\u0434\u044B \u0434\u0440\u0443\u0433 \u0441 \u0434\u0440\u0443\u0433\u043E\u043C \u043F\u043B\u043E\u0445\u043E \u043A\u043E\u043D\u0442\u0430\u0447\u0430\u0442.\n\u003C/p\u003E\n\u003Cp\u003E\n\u00A0\n\u003C/p\u003E\n\u003Cspan class=\u0022ipsType_reset ipsType_medium ipsType_light\u0022 data-excludequote=\u0022\u0022\u003E\n\u003Cstrong\u003EEdited \u003Ctime datetime=\u00222019-03-21T12:38:28Z\u0022 title=\u002203/21/2019 12:38  PM\u0022 data-short=\u00225 yr\u0022\u003EMarch 21, 2019\u003C/time\u003E by Sokol_323\u003C/strong\u003E\n\u003C/span\u003E\n"},{"CreatedByName":"-MadMan-","CreatedById":22353,"CreatedDateTime":"2019-04-16T09:33:37Z","Content":"\n\u003Cblockquote class=\u0022ipsQuote\u0022 data-ipsquote=\u0022\u0022 data-ipsquote-contentapp=\u0022forums\u0022 data-ipsquote-contentclass=\u0022forums_Topic\u0022 data-ipsquote-contentcommentid=\u00223564993\u0022 data-ipsquote-contentid=\u0022182941\u0022 data-ipsquote-contenttype=\u0022forums\u0022 data-ipsquote-timestamp=\u00221553171566\u0022 data-ipsquote-userid=\u0022185655\u0022 data-ipsquote-username=\u0022Sokol_323\u0022\u003E\n\u003Cdiv class=\u0022ipsQuote_citation\u0022\u003E\nOn 3/21/2019 at 5:32 PM, Sokol_323 said:\n\u003C/div\u003E\n\u003Cdiv class=\u0022ipsQuote_contents\u0022\u003E\n\u003Cp\u003E\n\u0414\u0430\u043B\u0435\u0435 \u0440\u0430\u0437\u0432\u0435\u0448\u0438\u0432\u0430\u0435\u043C \u0442\u0430\u043A \u043F\u043E \u043D\u0430\u043A\u043B\u043E\u043D\u0435\u043D\u0438\u044E 15-30 \u0433\