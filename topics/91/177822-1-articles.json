\u003E\n\u003Cp\u003E\n\u0418\u0445 \u0440\u0435\u0448\u0430\u044E\u0442?\n\u003C/p\u003E\n\u003C/div\u003E\n\u003C/blockquote\u003E\n\u003Cp\u003E\n\u041A\u043E\u0433\u0434\u0430 - \u0432 92 \u0433\u043E\u0434\u0443, \u043A\u043E\u0433\u0434\u0430 \u043F\u0440\u0438 \u043B\u0438\u043A\u0432\u0438\u0434\u0430\u0446\u0438\u0438 \u041C\u041E\u041C \u0432 \u0420\u041A\u0410 \u043D\u0435 \u043F\u0435\u0440\u0435\u0448\u043B\u0438 \u0443\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0438\u044F \u043E\u0442\u0432\u0435\u0447\u0430\u044E\u0449\u0438\u0435 \u0437\u0430 \u043A\u043E\u043E\u0440\u0434\u0438\u043D\u0430\u0446\u0438\u044E \u043F\u0440\u043E\u0438\u0437\u0432\u043E\u0434\u0441\u0442\u0432\u0430 \u0438 \u0441\u043C\u0435\u0436\u043D\u0438\u043A\u043E\u0432. \u0422.\u0435. \u0441 \u0441\u0430\u043C\u043E\u0433\u043E \u0022\u0440\u043E\u0436\u0434\u0435\u043D\u0438\u044F\u0022 \u0442\u043E, \u0447\u0442\u043E \u043F\u043E\u0442\u043E\u043C \u0441\u0442\u0430\u043B\u043E \u0420\u043E\u0441\u043A\u043E\u0441\u043C\u043E\u0441\u043E\u043C \u043D\u0435 \u0438\u043C\u0435\u043B\u043E \u043A\u043E\u043D\u0442\u0440\u043E\u043B\u044F \u043D\u0430\u0434 \u0442\u0435\u043C\u0438, \u043A\u0442\u043E \u0435\u043C\u0443 \u0437\u0430\u043A\u0430\u0437\u044B \u0440\u0435\u0430\u043B\u0438\u0437\u043E\u0432\u044B\u0432\u0430\u043B. \u041A\u043E\u0435-\u043A\u0430\u043A \u043F\u043E\u0440\u044F\u0434\u043E\u043A \u043D\u0430\u0432\u0435\u043B\u0438 \u043A \u043A\u043E\u043D\u0446\u0443 90\u0442\u044B\u0445 \u043D\u043E \u0435\u0434\u0438\u043D\u043E\u0433\u043E \u0446\u0435\u043D\u0442\u0440\u0430 \u043A\u043E\u043E\u0440\u0434\u0438\u043D\u0430\u0446\u0438\u0438 \u0442\u0430\u043A \u0438 \u043D\u0435 \u0431\u044B\u043B\u043E - \u0432\u0441\u0435 \u043F\u0440\u0435\u0434\u043F\u0440\u0438\u044F\u0442\u0438\u044F \u0436\u0440\u0430\u043B\u0438 \u0434\u0440\u0443\u0433-\u0434\u0440\u0443\u0433\u0430 \u0438 \u0432\u043E\u0435\u0432\u0430\u043B\u0438 \u0437\u0430 \u0437\u0430\u043A\u0430\u0437\u044B. \u041F\u0438\u043A\u043E\u043C \u0441\u0442\u0430\u043B\u0430 \u0432\u0435\u043D\u0434\u0435\u0442\u0442\u0430 \u043C\u0435\u0436\u0434\u0443 \u0426\u0438\u0425\u043E\u043C \u0438 \u0420\u041A\u041A\u042D,\u00A0\u0432 \u043A\u043E\u0442\u043E\u0440\u043E\u0439 \u043D\u0435 \u043F\u043E\u0431\u0435\u0434\u0438\u043B \u043D\u0438\u043A\u0442\u043E - \u0421\u043E\u043B\u043D\u0446\u0435\u0432 \u0441\u043D\u044F\u043B\u0438, \u043F\u0440\u0435\u0434\u044B\u0434\u0443\u0449\u0438\u0439 \u0434\u0438\u0440\u0435\u043A\u0442\u043E\u0440 \u0426\u0438\u0425\u0430 \u0438 \u0442\u0430\u043A \u043F\u043E\u0434 \u0441\u043B\u0435\u0434\u0441\u0442\u0432\u0438\u0435\u043C. \u0410\u043D\u0433\u0430\u0440\u0430 \u043D\u0435 \u043B\u0435\u0442\u0430\u0435\u0442, \u0424\u0435\u0434\u0435\u0440\u0430\u0446\u0438\u044E \u043F\u0440\u0438\u0448\u043B\u043E\u0441\u044C \u043F\u0435\u0440\u0435\u0441\u0430\u0436\u0438\u0432\u0430\u0442\u044C \u043D\u0430 \u043D\u043E\u0432\u0443\u044E \u0440\u0430\u043A\u0435\u0442\u0443 (\u0442\u0440\u0435\u0442\u0438\u0439 \u0440\u0430\u0437) \u0438 \u0442.\u0434. \u0438 \u0442.\u043F.\n\u003C/p\u003E\n\u003Cp\u003E\n\u041E\u0442 \u0440\u0443\u043A\u043E\u0432\u043E\u0434\u0441\u0442\u0432\u0430 \u0438 \u0440\u0435\u0441\u0442\u0440\u0443\u043A\u0442\u0443\u0440\u0438\u0437\u0430\u0446\u0438\u0438 \u0430\u0433\u0435\u043D\u0441\u0442\u0432\u0430 \u0432 \u0433\u043E\u0441\u043A\u043E\u0440\u043F\u043E\u0440\u0430\u0446\u0438\u044E \u043C\u0430\u043B\u043E \u0447\u0442\u043E \u0437\u0430\u0432\u0438\u0441\u0438\u0442 - \u0432\u0441\u0435 \u0440\u0435\u0448\u0438\u0442\u0441\u044F \u0437\u0430 \u0441\u0447\u0435\u0442 \u043B\u044E\u0434\u0435\u0439, \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043F\u0440\u0438\u0448\u043B\u0438 \u0432 \u043A\u043E\u0441\u043C\u043E\u043D\u0430\u0432\u0442\u0438\u043A\u0443 \u0432 \u043D\u0443\u043B\u0435\u0432\u044B\u0435 \u0438 \u0441\u0435\u0439\u0447\u0430\u0441 \u043D\u0430 \u0434\u043E\u043B\u0436\u043D\u