2\u0430\u043A(\u0437\u0430\u0445\u0432\u0430\u0442 \u0434\u043E\u043B\u0436\u0435\u043D \u0431\u044B\u0442\u044C \u0430\u043A\u0442\u0438\u0432\u0438\u0440\u043E\u0432\u0430\u043D), \u0447\u0442\u043E\u0431 \u043E\u043D\u0430 \u043E\u043A\u0430\u0437\u0430\u043B\u0430\u0441\u044C \u0432\u043D\u0443\u0442\u0440\u0438 \u043E\u0442\u0441\u0435\u043A\u0430 \u0432 \u043D\u0443\u0436\u043D\u043E\u043C \u043D\u0430\u043C \u0440\u0430\u0441\u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0438, \u0436\u043C\u0435\u043C \u0433\u0430\u0437\u0443\u043B\u044C\u043A\u0443 \u0438 \u0446\u0435\u043B\u044C \u0432\u043B\u0435\u043F\u043B\u044F\u0435\u0442\u0441\u044F \u0432 \u0437\u0430\u0445\u0432\u0430\u0442 \u0438 \u0445\u0432\u0430\u0442\u0430\u0435\u0442\u0441\u044F \u0438\u043C. \u0412\u0441\u0435.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0415\u0441\u043B\u0438 \u043F\u043E\u043B\u044C\u0437\u0443\u0435\u043C\u0441\u044F \u043C\u043E\u0434\u0430\u043C\u0438, \u043C\u043E\u0436\u043D\u043E \u0441\u0434\u0435\u043B\u0430\u0442\u044C \u043F\u0440\u043E\u0449\u0435. \u041F\u043E\u0434\u043B\u0435\u0442\u0430\u0435\u043C \u043A \u0446\u0435\u043B\u0438 \u0432 \u043D\u0443\u0436\u043D\u043E\u043C \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0438, \u0430 \u0434\u0430\u043B\u044C\u0448\u0435 \u0432\u043C\u0435\u0441\u0442\u043E \u0442\u043E\u0447\u043D\u043E\u0433\u043E \u043C\u0430\u043D\u0435\u0432\u0440\u0438\u0440\u043E\u0432\u0430\u043D\u0438\u044F \u0438 \u0437\u0430\u0432\u043E\u0434\u0430 \u0435\u0435 \u0432 \u0433\u0440\u0443\u0437\u043E\u0432\u043E\u0439 \u043E\u0442\u0441\u0435\u043A... \u0423 \u043D\u0430\u0441 \u043D\u0430\u043F\u0440\u043E\u0442\u0438\u0432 \u0437\u0430\u0445\u0432\u0430\u0442\u0430 \u0432 \u0433\u0440\u0443\u0437\u043E\u0432\u043E\u043C \u043E\u0442\u0441\u0435\u043A\u0435 \u0434\u043E\u043B\u0436\u043D\u0430 \u0443\u0436\u0435 \u0441\u0442\u043E\u044F\u0442\u044C \u043B\u0435\u0431\u0435\u0434\u043A\u0430 \u0438\u0437 \u003Cabbr title=\u0022Kerbal Attachment System (mod)\u0022\u003EKAS\u003C/abbr\u003E, \u043F\u043E\u0434\u043B\u0435\u0442\u0430\u0435\u043C \u043A\u0435\u0440\u0431\u043E\u043D\u0430\u0432\u0442\u043E\u043C \u043A \u0446\u0435\u043B\u0438, \u0441\u043E \u0441\u0442\u043E\u0440\u043E\u043D\u044B \u043D\u0430\u043F\u0440\u043E\u0442\u0438\u0432 \u0442\u043E\u0433\u043E \u043C\u0435\u0441\u0442\u0430, \u0433\u0434\u0435 \u043C\u044B \u0431\u0443\u0434\u0435\u043C \u0445\u0432\u0430\u0442\u0430\u0442\u044C \u0446\u0435\u043B\u044C \u0437\u0430\u0445\u0432\u0430\u0442\u043E\u043C, \u043A\u0440\u0435\u043F\u0438\u043C \u043D\u0430 \u0446\u0435\u043B\u044C\u00A0\u0441\u0442\u044B\u043A\u043E\u0432\u043E\u0447\u043D\u044B\u0439 \u0443\u0437\u0435\u043B(\u043D\u0435 \u043F\u043E\u043C\u043D\u044E \u043A\u0430\u043A \u043D\u0430\u0437\u044B\u0432\u0430\u0435\u0442\u0441\u044F) \u0438\u0437 \u003Cabbr title=\u0022Kerbal Attachment System (mod)\u0022\u003EKAS\u003C/abbr\u003E, \u043F\u043E\u0442\u043E\u043C \u0445\u0432\u0430\u0442\u0430\u0435\u043C \u043A\u043E\u043D\u0435\u0446 \u0442\u0440\u043E\u0441\u0430 \u043E\u0442 \u043B\u0435\u0431\u0435\u0434\u043A\u0438, \u0446\u0435\u043F\u043B\u044F\u0435\u043C \u0435\u0433\u043E \u043A \u044D\u0442\u043E\u043C\u0443 \u0443\u0437\u043B\u0443, \u0438 \u0438\u0434\u0435\u043C \u043E\u0431\u0440\u0430\u0442\u043D\u043E \u0432 \u043A\u0430\u0431\u0438\u043D\u0443. \u0422\u044F\u043D\u0435\u043C \u0442\u0440\u043E\u0441, \u0446\u0435\u043B\u044C \u043F\u0440\u0438\u0442\u044F\u0433\u0438\u0432\u0430\u0435\u0442\u0441\u044F \u0432 \u043E\u0442\u0441\u0435\u043A \u0438 \u0441\u0442\u0430\u043D\u043E\u0432\u0438\u0442\u0441\u044F \u0432 \u043D\u0443\u0436\u043D\u043E\u043C \u043D\u0430\u043C \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0438(\u0435\u0441\u043B\u0438 \u0446\u0435\u043B\u044C \u0442\u044F\u0436\u0435\u043B\u0430\u044F \u0442\u043E \u043D\u0430\u0434\u043E \u0441\u043A\u043E\u0440\u0440\u0435\u043A\u0442\u0438\u0440\u043E\u0432\u0430\u0442\u044C \u043F\u043E\u043B\u043E\u0436\u0435\u043D\u0438\u0435 \u043F\u0435\u043F\u0435\u043B\u0430\u0446\u0430 \u043E\u0442\u043D\u043E\u0441\u0438\u0442\u0435\u043B\u044C\u043D\u043E \u0437\u0430\u0442\u044F\u043D\u0443\u0442\u043E\u0439 \u0446\u0435\u043B\u0438 \u043C\u0430\u043D\u0435\u0432\u0440\u043E\u0432\u044B\u043C\u0438), \u043E\u0441\u043B\u0430\u0431\u043B\u044F\u0435\u043C \u0442\u0440\u043E\u0441 \u0438 \u0434\u0430\u0435\u043C \u0433\u0430\u0437\u0443\u043B\u044C\u043A\u0443 - \u0446\u0435\u043B\u044C \u0432\u043B\u0435\u043F\u043B\u044F\u0435\u0442\u0441\u044F \u0432 \u0437\u0430\u0445\u0432\u0430\u0442. \u0412\u0441\u0435. \u0414\u043B\u044F \u0434\u043E\u043F\u043E\u043B\u043D\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0439 \u0444\u0438\u043A\u0441\u0430\u0446\u0438\u0438 \u0433\u0440\u0443\u0437\u0430 \u043C\u043E\u0436\u043D\u043E \u043D\u0430\u0442\u044F\u043D\u0443\u0442\u044C \u0442\u0440\u043E\u0441. \u041B\u0435\u0442\u0438\u043C \u043E\u0431\u0440\u0430\u0442\u043D\u043E.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0412\u0438\u0434\u0435\u043B \u0432\u0438\u0434\u043E\u0441\u044B \u0447\u0442\u043E \u043B\u0435\u0442\u0430\u044E\u0442 \u043E\u0431\u044B\u0447\u043D\u044B\u043C\u0438 \u0440\u0430\u043A\u0435\u0442\u0430\u043C\u0438 \u0438 \u0432\u043E\u0437\u0432\u0440\u0430\u0449\u0430\u044E\u0442\u0441\u044F \u0433\u0440\u0443\u0437\u043E\u0432\u044B\u043C\u0438 \u043C\u043E\u0434\u0443\u043B\u044F\u043C\u0438 \u0438\u043B\u0438 \u0432\u043E\u043E\u0431\u0449\u0435 \u0442\u0443\u043F\u043E \u043C\u043E\u0434\u0443\u043B\u0435\u043C \u0443\u043F\u0440\u0430\u0432\u043B\u0435\u043D\u0438\u044F \u0441 \u0437\u0430\u0445\u0432\u0430\u0442\u043E\u043C \u0438 \u0446\u0435\u043B\u044C\u044E \u043D\u0430 \u043F\u0430\u0440\u0430\u0448\u044E\u0442\u0430\u0445 - \u043C\u043E\u0436\u043D\u043E \u0438 \u0442\u0430\u043A, \u043D\u043E \u0438\u043C\u0445\u043E \u044D\u0442\u043E \u043D\u0435 \u0442\u0440\u0443\u042A \u0438 \u043F\u043E \u043B\u0430\u043C\u0435\u0440\u0441\u043A\