\u0431\u0449\u0435\u043D\u0438\u044F\u043C\u0438, \u043E\u043F\u043E\u0432\u0435\u0449\u0430\u0442\u044C \u0432\u0441\u0435\u0445 \u0443\u0447\u0430\u0441\u0442\u043D\u0438\u043A\u043E\u0432 \u0441\u0435\u0440\u0432\u0435\u0440\u0430. \u0415\u0441\u043B\u0438 \u0432\u044B \u0445\u043E\u0442\u0438\u0442\u0435 \u0440\u0430\u0441\u043F\u0440\u043E\u0441\u0442\u0440\u0430\u043D\u044F\u0442\u044C \u043D\u043E\u0432\u043E\u0441\u0442\u0438 \u0410\u043D\u0433\u0430\u0440\u0430 \u0414\u0436\u0435\u0431\u0430 \u0438 \u043D\u0435 \u0442\u043E\u043B\u044C\u043A\u043E, \u043F\u0438\u0448\u0438\u0442\u0435 \u0432 \u043A\u0430\u043D\u0430\u043B jebs_hangar_chat, \u043C\u044B \u0431\u0443\u0434\u0435\u043C \u0440\u0430\u0434\u044B!\u003C/span\u003E\u003C/strong\u003E\n\u003C/p\u003E\n"},{"CreatedByName":"Davian Lin","CreatedById":162286,"CreatedDateTime":"2017-08-22T12:25:16Z","Content":"\n\u003Cp\u003E\n\u003Cu\u003E\u003Cstrong\u003E\u0410\u043D\u0433\u0430\u0440 \u0414\u0436\u0435\u0431\u0430 \u043E\u0431\u044A\u044F\u0432\u043B\u044F\u0435\u0442 \u043D\u043E\u0432\u044B\u0439 \u043A\u043E\u043D\u043A\u0443\u0440\u0441!\u003C/strong\u003E\u003C/u\u003E\u003Cbr\u003E\n\u0412 \u0441\u0432\u044F\u0437\u0438 \u0441 \u0430\u043A\u0446\u0438\u0435\u0439 \u0410\u043D\u0433\u0430\u0440 \u0414\u0436\u0435\u0431\u0430 \u0440\u0430\u0437\u044B\u0433\u0440\u044B\u0432\u0430\u0435\u0442 \u043A\u043E\u043D\u043A\u0443\u0440\u0441.\u003Cbr\u003E\n\u0412\u0430\u043C \u043D\u0443\u0436\u043D\u043E: 1. \u041F\u043E\u0441\u0442\u0440\u043E\u0438\u0442\u044C \u043A\u0440\u0430\u0444\u0442, \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u0441\u043C\u043E\u0436\u0435\u0442 \u0434\u043E\u043B\u0435\u0442\u0435\u0442\u044C \u0434\u043E \u041C\u0443\u043D\u044B \u0438\u043B\u0438 \u041C\u0438\u043D\u043C\u0443\u0441\u0430.\u003Cbr\u003E\n2. \u041A\u0440\u0430\u0444\u0442 \u0434\u043E\u043B\u0436\u0435\u043D \u0438\u043C\u0435\u0442\u044C \u044D\u043A\u0438\u043F\u0430\u0436 \u043D\u0435 \u043C\u0435\u043D\u0435\u0435 \u0447\u0435\u043C \u0438\u0437 5 \u043A\u0435\u0440\u0431\u043E\u043D\u0430\u0432\u0442\u043E\u0432.\u003Cbr\u003E\n3. \u041E\u0441\u043D\u043E\u0432\u0430\u0442\u044C \u043D\u0430 \u043F\u043B\u0430\u043D\u0435\u0442\u0435 \u043D\u0435\u0431\u043E\u043B\u044C\u0448\u0443\u044E \u0431\u0430\u0437\u0443 (\u0438\u043B\u0438 \u0436\u0438\u043B\u043E\u0439 \u043C\u043E\u0434\u0443\u043B\u044C \u0434\u043B\u044F \u0432\u0440\u0435\u043C\u0435\u043D\u043D\u043E\u0433\u043E \u043E\u0431\u0438\u0442\u0430\u043D\u0438\u044F).\u003Cbr\u003E\n4. \u0418 \u0431\u043B\u0430\u0433\u043E\u043F\u043E\u043B\u0443\u0447\u043D\u043E \u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F \u0434\u043E\u043C\u043E\u0439.\u003Cbr\u003E\u003Cbr\u003E\u003Cem\u003E\u0412\u044B \u043D\u0435 \u0434\u043E\u043B\u0436\u043D\u044B \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C \u043C\u043E\u0434\u044B, \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u043C\u043E\u0433\u0443\u0442 \u043F\u043E\u0432\u043B\u0438\u044F\u0442\u044C \u043D\u0430 \u0438\u0433\u0440\u043E\u0432\u043E\u0439 \u0433\u0435\u0439\u043C\u043F\u043B\u0435\u0439, \u0438 \u0434\u0435\u0442\u0430\u043B\u0438. \u0422\u0430\u043A\u0436\u0435 \u0437\u0430\u043F\u0440\u0435\u0449\u0435\u043D\u043E \u0438\u0441\u043F\u043E\u043B\u044C\u0437\u043E\u0432\u0430\u0442\u044C \u043A\u043E\u043D\u0441\u043E\u043B\u044C \u0432 \u0438\u0433\u0440\u0435 (Alt\u002BF12 \u0438\u043B\u0438 \u0436\u0435 Hyper Edit).\u003C/em\u003E \u003Cem\u003E\u003Cstrong\u003E\u041D\u043E! \u041F\u0440\u0438\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044E\u0442\u0441\u044F \u043C\u043E\u0434\u044B TACLF, Trajectories, \u0430 \u0442\u0430\u043A\u0436\u0435 \u0440\u0430\u0437\u043D\u044B\u0435 \u0433\u0440\u0430\u0444\u0438\u0447\u0435\u0441\u043A\u0438\u0435 \u043C\u043E\u0434\u044B.\u003C/strong\u003E\u003C/em\u003E \u041F\u043E \u0445\u043E\u0434\u0443 \u043F\u043E\u043B\u0435\u0442\u0430 \u0432\u044B \u0434\u043E\u043B\u0436\u043D\u044B \u043F\u0440\u0438\u0441\u044B\u043B\u0430\u0442\u044C \u0441\u043A\u0440\u0438\u043D\u0448\u043E\u0442\u044B: \u0441\u0442\u0430\u0440\u0442 \u0441 \u041A\u0435\u0440\u0431\u0438\u043D\u0430, \u0443\u0445\u043E\u0434 \u0441 \u043E\u0440\u0431\u0438\u0442\u044B \u041A\u0435\u0440\u0431\u0438\u043D\u0430, \u0442\u0440\u0430\u043D\u0441\u0444\u0435\u0440 \u043A \u0437\u0430\u0434\u0430\u043D\u043D\u043E\u0439 \u043F\u043B\u0430\u043D\u0435\u0442\u0435, \u043F\u043E\u0434\u043B\u0435\u0442 \u043A \u043D\u0435\u0439, \u043F\u043E\u0441\u0430\u0434\u043A\u0430, \u043E\u0441\u043D\u043E\u0432\u0430\u043D\u0438\u0435 \u0431\u0430\u0437\u044B, \u0442\u0440\u0430\u043D\u0441\u0444\u0435\u0440 \u043A \u041A\u0435\u0440\u0431\u0438\u043D\u0443, \u043F\u043E\u0434\u043B\u0435\u0442 \u043A \u043D\u0435\u043C\u0443 \u0438 \u043F\u043E\u0441\u0430\u0434\u043A\u0430. \u0421\u043A\u0440\u0438\u043D\u0448\u043E\u0442\u044B \u043F\u0440\u0438\u0441\u044B\u043B\u0430\u0442\u044C \u0432 \u043A\u0430\u043D\u0430\u043B #jebs_hangar_chat. \u0422\u0440\u043E\u0435 \u043D\u0430\u0438\u043B\u0443\u0447\u0448\u0438\u0445 \u0443\u0447\u0430\u0441\u0442\u043D\u0438\u043A\u043E\u0432 \u0441\u043E \u0441\u0432\u043E\u0438\u043C\u0438 \u0440\u0435\u0437\u0443\u043B\u044C\u0442\u0430\u0442\u0430\u043C\u0438 \u0438 \u043A\u0440\u0430\u0444\u0442\u0430\u043C\u0438 \u0431\u0443\u0434\u0443\u0442 \u043D\u0430\u0433\u0440\u0430\u0436\u0434\u0435\u043D\u044B \u043E\u0442 \u0410\u0434\u043C\u0438\u043D\u0438\u0441\u0442\u0440\u0430\u0446\u0438\u0438 \u0441\u0435\u0440\u0432\u0435\u0440\u0430 \u0440\u043E\u043B\u044C\u044E \u003Cstrong\u003E@\u0425\u043E\u0440\u043E\u0448\u0438\u0435 \u043B\u044E\u0434\u0438\u003C/strong\u003E \u0438\u043B\u0438 \u003Cstrong\u003E@\u0418\u0437\u0431\u0440\u0430\u043D\u043D\u044B\u0435 \u043B\u0438\u0447\u043D\u043E\u0441\u0442\u0438\u003C/strong\u003E. \u003Cstrong\u003E\u0414\u0435\u0440\u0437\u0430\u0439\u0442\u0435!\u003Cbr\u003E\n\u0421\u0441\u044B\u043B\u043