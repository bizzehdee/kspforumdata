43E\u0437\u0432\u0440\u0430\u0442 \u0026gt;80% \u0441\u0442\u043E\u0438\u043C\u043E\u0441\u0442\u0438 \u043A\u043E\u0440\u0430\u0431\u043B\u044F \u0437\u0430 \u0432\u044B\u0447\u0435\u0442\u043E\u043C \u0442\u043E\u043F\u043B\u0438\u0432\u0430.\u00A0 \u041B\u0438\u0431\u043E \u0432\u044B\u043F\u043E\u043B\u043D\u044F\u0442\u044C \u043A\u043E\u043D\u0442\u0440\u0430\u043A\u0442\u044B \u0438\u043C\u0435\u044E\u0449\u0438\u043C\u0441\u044F \u043D\u0430 \u043E\u0440\u0431\u0438\u0442\u0435 \u0445\u043B\u0430\u043C\u043E\u043C.\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u041F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u043B \u0432\u0438\u0434\u0435\u043E \u043F\u043E \u043C\u043E\u0434\u0443 GS - \u044D\u0442\u043E \u0443\u0436 \u0441\u0438\u043B\u044C\u043D\u043E \u0444\u0430\u043D\u0442\u0430\u0441\u0442\u0438\u0447\u043D\u043E...\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u041F\u0440\u043E \u043A\u043D\u043E\u043F\u043A\u0443 \u0022\u0443\u043D\u0438\u0447\u0442\u043E\u0436\u0438\u0442\u044C\u0022 \u0422\u0421 \u0432 \u043A\u0443\u0440\u0441\u0435. \u041F\u0443\u0441\u0442\u044B\u0435 \u043E\u0442\u0441\u0435\u043A\u0438 \u043F\u043E\u0441\u043B\u0435 \u0441\u043F\u0430\u0441\u0435\u043D\u0438\u044F \u043A\u0435\u0440\u0431\u0430\u043B\u043E\u0432 \u0443\u0431\u0438\u0440\u0430\u044E. \u0410 \u0432\u043E\u0442 \u0430\u0442\u043E\u043C\u043D\u044B\u0435 \u0434\u0432\u0438\u0436\u043A\u0438 \u0441 \u043E\u0431\u0432\u0435\u0441\u043E\u043C \u0443\u0436\u0435 \u043E\u0431\u0438\u0434\u043D\u043E \u0442\u0435\u0440\u044F\u0442\u044C...\n\u003C/p\u003E\n\u003Cp\u003E\nUPD: \u041D\u0443 \u0438\u043B\u0438 \u043A \u043F\u0440\u0438\u043C\u0435\u0440\u0443, \u0431\u044B\u043B \u0437\u0430\u043A\u0430\u0437 \u043D\u0430 4 \u0442\u0443\u0440\u0438\u0441\u0442\u043E\u0432 \u0441 \u043F\u043E\u0441\u0430\u0434\u043A\u043E\u0439 \u043D\u0430 \u041C\u0443\u043D\u0435. \u0441\u0434\u0435\u043B\u0430\u043B \u0441\u043F\u0443\u0441\u043A\u0430\u0435\u043C\u044B\u0439 \u0430\u043F\u043F\u0430\u0440\u0430\u0442 \u043D\u0430 6 \u043C\u0435\u0441\u0442 (\u043F\u0438\u043B\u043E\u0442 \u002B \u0438\u043D\u0436\u0435\u043D\u0435\u0440, \u0442.\u043A. \u0431\u044B\u043B \u043F\u0435\u0447\u0430\u043B\u044C\u043D\u044B\u0439 \u043E\u043F\u044B\u0442 \u0443\u0440\u043E\u043D\u0438\u043B \u0430\u043F\u043F\u0430\u0440\u0430\u0442, \u0441\u043B\u043E\u043C\u0430\u043B \u0441\u043E\u043B\u043D\u0435\u0447\u043D\u044B\u0435 \u043F\u0430\u043D\u0435\u043B\u0438, \u043F\u043E\u043A\u0430 \u043F\u043E\u0434\u043D\u0438\u043C\u0430\u043B - \u043A\u043E\u043D\u0447\u0438\u043B\u0438\u0441\u044C \u0431\u0430\u0442\u0430\u0440\u0435\u0439\u043A\u0438, \u043D\u0435 \u0441\u043C\u043E\u0433 \u0432\u0437\u043B\u0435\u0442\u0435\u0442\u044C. \u0418\u043D\u0436\u0435\u043D\u0435\u0440 \u0432\u0440\u043E\u0434\u0435 \u0434\u043E\u043B\u0436\u0435\u043D \u0443\u043C\u0435\u0442\u044C \u0440\u0435\u043C\u043E\u043D\u0442\u0438\u0440\u043E\u0432\u0430\u0442\u044C, \u043D\u043E \u043F\u043E\u043A\u0430 \u043D\u0435 \u043F\u0440\u0438\u0433\u043E\u0434\u0438\u043B\u0441\u044F). \u0421\u0435\u0439\u0447\u0430\u0441 \u043D\u0443\u0436\u043D\u043E \u0441\u043F\u0443\u0441\u0442\u0438\u0442\u044C 1 \u0443\u0447\u0435\u043D\u043E\u0433\u043E \u0441 \u043E\u0440\u0431\u0438\u0442\u044B \u041C\u0443\u043D\u044B \u0438 \u043F\u043E\u0442\u043E\u043C \u0432\u0435\u0440\u043D\u0443\u0442\u044C \u043E\u0431\u0440\u0430\u0442\u043D\u043E. \u0420\u044F\u0434\u043E\u043C \u0435\u0441\u0442\u044C \u0441\u043F\u0443\u0442\u043D\u0438\u043A \u0441 \u043C\u043E\u0434\u0443\u043B\u0435\u043C \u0413\u0415\u041A\u0421. \u0412\u044B\u043A\u0438\u043D\u0443\u0442\u044C \u0432\u0441\u0435 \u043B\u0438\u0448\u043D\u0435\u0435, \u043E\u0441\u0442\u0430\u0432\u0438\u0442\u044C \u043A\u0430\u043F\u0441\u0443\u043B\u0443, \u0413\u0415\u041A\u0421 \u0438 \u0422\u0435\u0440\u044C\u0435\u0440\u0430 \u0434\u043B\u044F \u044D\u043A\u043E\u043D\u043E\u043C\u0438\u0438 \u0442\u043E\u043F\u043B\u0438\u0432\u0430. \u0417\