043E\u0433\u043E \u043D\u0430\u0447\u0430\u043B\u0430 , \u0430 \u0438\u043C\u0435\u043D\u043D\u043E \u0441 \u043F\u043E\u043B\u0451\u0442\u0430 \u043D\u0430 \u0430\u044D\u0440\u043E\u043F\u043E\u0440\u0442 \u043D\u0430 \u043E\u0441\u0442\u0440\u043E\u0432.\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u042F \u0432\u043E\u0442 \u0441\u043E\u0432\u0441\u0435\u043C \u043D\u0435 \u043C\u043E\u0433\u0443 \u043F\u043E\u043D\u044F\u0442\u044C, \u043A\u0430\u043A \u043C\u043D\u0435 \u0443\u0434\u043E\u0431\u043D\u043E \u0432\u0435\u0440\u043D\u0443\u0442\u044C\u0441\u044F \u0432 \u0430\u043D\u0433\u0430\u0440? \u0418\u043C\u0435\u043D\u043D\u043E \u0432 \u0440\u0435\u0436\u0438\u043C\u0435 \u043C\u0438\u0441\u0441\u0438\u0439 \u044F \u043F\u043E\u0447\u0435\u043C\u0443-\u0442\u043E \u0442\u0430\u043A\u043E\u0439 \u0432\u043E\u0437\u043C\u043E\u0436\u043D\u043E\u0441\u0442\u0438 \u043D\u0435 \u0432\u0438\u0436\u0443.\n\u003C/p\u003E\n\u003Cp\u003E\n\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u0414\u0430\u043B\u0435\u0435, \u0432\u043E \u0432\u0442\u043E\u0440\u043E\u0439 \u043C\u0438\u0441\u0441\u0438\u0438 , \u0442\u0430\u043C \u0433\u0434\u0435 \u043D\u0430 \u043E\u0440\u0431\u0438\u0442\u0435 \u041A\u0435\u0440\u0431\u0430\u043B\u0430 \u043A\u0430\u043A\u0430\u044F-\u0442\u043E \u0430\u0432\u0430\u0440\u0438\u044F \u043F\u0440\u043E\u0438\u0437\u043E\u0448\u043B\u0430, \u0438 \u043D\u0430\u0434\u043E \u043F\u0435\u0440\u0435\u043B\u0435\u0441\u0442\u044C \u043D\u0430 \u0434\u0440\u0443\u0433\u043E\u0439 \u043A\u043E\u0440\u0430\u0431\u043B\u044C. \u0422\u0430\u043A \u0432\u043E\u0442, \u043F\u043E\u0441\u043B\u0435 \u0432\u044B\u0445\u043E\u0434\u0430 \u043A\u0435\u0440\u0431\u043E\u043D\u0430\u0432\u0442\u043E\u043C \u0432 \u043E\u0442\u043A\u0440\u044B\u0442\u044B\u0439 \u043A\u043E\u0441\u043C\u043E\u0441 \u043F\u043E\u0447\u0435\u043C\u0443 \u043D\u0435 \u0440\u0430\u0431\u043E\u0442\u0430\u0435\u0442 \u0435\u0433\u043E \u0022\u0440\u0430\u043D\u0435\u0446\u0022 \u0438\u043B\u04