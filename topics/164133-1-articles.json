span\u003E\u003Cspan class=\u0022tag\u0022\u003E\u0026lt;summary\u0026gt;\u003C/span\u003E\u003Cspan class=\u0022pln\u0022\u003E\n        /// Called when this module is deactivated.\n        /// \u003C/span\u003E\u003Cspan class=\u0022tag\u0022\u003E\u0026lt;/summary\u0026gt;\u003C/span\u003E\u003Cspan class=\u0022pln\u0022\u003E\n        public void OnDisable()\n        {\n            GameEvents.onEditorPartPlaced.Remove(OnPartPlaced);\n        }\n\n\n        /// \u003C/span\u003E\u003Cspan class=\u0022tag\u0022\u003E\u0026lt;summary\u0026gt;\u003C/span\u003E\u003Cspan class=\u0022pln\u0022\u003E\n        /// Called when a part is attached.\n        /// \u003C/span\u003E\u003Cspan class=\u0022tag\u0022\u003E\u0026lt;/summary\u0026gt;\u003C/span\u003E\u003Cspan class=\u0022pln\u0022\u003E\n        /// \u003C/span\u003E\u003Cspan class=\u0022tag\u0022\u003E\u0026lt;param\u003C/span\u003E\u003Cspan class=\u0022pln\u0022\u003E \u003C/span\u003E\u003Cspan class=\u0022atn\u0022\u003Ename\u003C/span\u003E\u003Cspan class=\u0022pun\u0022\u003E=\u003C/span\u003E\u003Cspan class=\u0022atv\u0022\u003E\u0022data\u0022\u003C/span\u003E\u003Cspan class=\u0022tag\u0022\u003E\u0026gt;\u003C/span\u003E\u003Cspan class=\u0022pln\u0022\u003EThe data.\u003C/span\u003E\u003Cspan class=\u0022tag\u0022\u003E\u0026lt;/param\u0026gt;\u003C/span\u003E\u003Cspan class=\u0022pln\u0022\u003E\n        public void OnPartPlaced(Part data)\n        {\n            ScreenMessages.PostScreenMessage(\u0022PartAttached()\u0022);\u003C/span\u003E\u003C/pre\u003E\n\u003Cp\u003E\n\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\nThe screen message is never displayed which means the method isn\u0027t being called.\n\u003C/p\u003E\n"},{"CreatedByName":"linuxgurugamer","CreatedById":129964,"CreatedDateTime":"2017-08-07T04:46:00Z","Content":"\n\u003Cp\u003E\nWould really need to see all the code\n\u003C/p\u003E\n"},{"CreatedByName":"Benjamin Kerman","CreatedById":172792,"CreatedDateTime":"2017-08-07T05:36:35Z","Content"