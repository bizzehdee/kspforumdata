043F\u0435\u0439\u043D\u0442\u0435 \u043D\u0435 \u0440\u0430\u0434\u0438 \u0432\u044B\u043A\u0440\u0443\u0442\u0430\u0441\u043E\u0432 \u0430\u043B\u044F \u0022\u0432\u0438\u0434\u0430\u043B\u0438 \u043A\u0430\u043A \u043C\u043E\u0433\u0443 \u0432 \u043F\u0440\u043E\u0441\u0442\u043E\u043C \u043F\u0440\u0438\u043B\u043E\u0436\u0435\u043D\u0438\u0438\u0022, \u0430 \u043E\u0442 \u0441\u043E\u0431\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0439 \u0442\u0443\u043F\u043E\u0441\u0442\u0438.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0423 \u043C\u0435\u043D\u044F \u043D\u0435\u0442 \u0442\u0430\u043B\u0430\u043D\u0442\u0430, \u043D\u0435\u0442 \u0441\u043E\u0431\u0441\u0442\u0432\u0435\u043D\u043D\u043E\u0433\u043E \u0441\u0442\u0438\u043B\u044F, \u043D\u0435\u0442 \u043F\u0440\u043E\u0433\u0440\u0435\u0441\u0441\u0430, \u0435\u0441\u0442\u044C \u0442\u043E\u043B\u044C\u043A\u043E \u043C\u0435\u0434\u043B\u0435\u043D\u043D\u0430\u044F \u0438 \u0431\u0435\u0437\u044B\u0441\u0445\u043E\u0434\u043D\u0430\u044F \u0434\u0435\u0433\u0440\u0430\u0434\u0430\u0446\u0438\u044F. \u041D\u0438\u0447\u0435\u0433\u043E \u043C\u043D\u0435 \u0431\u043E\u043B\u044C\u0448\u0435 \u043D\u0435 \u043F\u0440\u0438\u043D\u043E\u0441\u0438\u0442 \u0440\u0430\u0434\u043E\u0441\u0442\u0438. \u041C\u043E\u0438 \u043F\u043E\u0438\u0433\u0440\u0443\u043B\u044C\u043A\u0438 \u0432 \u0412\u0430\u0440\u0442\u0430\u043D\u0434\u0435\u0440 \u0438 \u041A\u0421\u041F \u043F\u0440\u0438\u043D\u043E\u0441\u044F\u0442 \u0442\u043E\u043B\u044C\u043A\u043E \u0440\u0430\u0437\u0434\u0440\u0430\u0436\u0435\u043D\u0438\u0435 \u0438 \u0440\u0430\u0437\u043E\u0447\u0430\u0440\u043E\u0432\u0430\u043D\u0438\u0435 \u0432 \u0441\u0435\u0431\u0435, \u043D\u0435\u0432\u0430\u0436\u043D\u043E \u0445\u043E\u0440\u043E\u0448\u043E \u043B\u0438 \u0443 \u043C\u0435\u043D\u044F \u0438\u0434\u0443\u0442 \u0434\u0435\u043B\u0430 \u0432 \u0438\u0433\u0440\u0435 \u0438\u043B\u0438 \u043D\u0435 \u043E\u0447\u0435\u043D\u044C.\u00A0\u042F \u0431\u0440\u043E\u0441\u0438\u043B \u043F\u0438\u0441\u0430\u0442\u044C \u0442\u043E\u0442 \u0434\u043E\u043B\u0431\u0430\u043D\u044B\u0439 \u0440\u0430\u0441\u0441\u043A\u0430\u0437, \u043A\u043E\u0442\u043E\u0440\u044B\u0439 \u0432 \u0441\u0432\u043E\u0435\u0439\u00A0\u043F\u0440\u0435\u0434-\u0444\u0438\u043D\u0430\u043B\u044C\u043D\u043E\u0439\u00A0\u0444\u043E\u0440\u043C\u0435 \u0431\u043E\u043B\u044C\u0448\u0435 \u043F\u043E\u0445\u043E\u0434\u0438\u043B \u043D\u0430 \u0431\u0440\u0435\u0434\u043D\u0438 \u0441\u0443\u043C\u0430\u0441\u0448\u0435\u0434\u0448\u0435\u0433\u043E. \u042F \u0432\u0441\u0435\u0433\u0434\u0430 \u043F\u043B\u043E\u0445\u043E \u043F\u0438\u0441\u0430\u043B, \u043D\u0435\u0441\u043C\u043E\u0442\u0440\u044F \u043D\u0430 \u0442\u043E \u0447\u0442\u043E \u044F \u0435\u0434\u0438\u043D\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u0439 \u043A\u0442\u043E \u043F\u0438\u0448\u0435\u0442 \u0432 \u043C\u043E\u0451\u043C \u043A\u043B\u0430\u0441\u0441\u0435. \u0412\u0438\u0434\u0438\u043C\u043E \u044D\u0442\u043E \u043C\u043E\u044F \u0444\u0438\u0448\u043A\u0430 - \u0431\u044B\u0442\u044C \u0435\u0434\u0438\u043D\u0441\u0442\u0432\u0435\u043D\u043D\u044B\u043C \u0438 \u0445\u0443\u0434\u0448\u0438\u043C.\n\u003C/p\u003E\n\u003Cp\u003E\n\u041D\u0438\u0447\u0435\u0433\u043E \u043D\u0435 \u043F\u0440\u0438\u043D\u043E\u0441\u0438\u0442 \u043C\u043D\u0435 \u0432\u0434\u043E\u0445\u043D\u043E\u0432\u0435\u043D\u0438\u0435. \u0420\u0430\u043D\u044C\u0448\u0435 \u044F \u043B\u044E\u0431\u0438\u043B \u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C \u043D\u0430 \u0440\u0430\u0431\u043E\u0442\u044B \u0434\u0440\u0443\u0433\u0438\u0445 \u0445\u0443\u0434\u043E\u0436\u043D\u0438\u043A\u043E\u0432 \u0438 \u043F\u0440\u043E\u0431\u043E\u0432\u0430\u0442\u044C \u043F\u043E\u0434\u0440\u0430\u0436\u0430\u0442\u044C \u0438\u043C. \u0421\u0435\u0439\u0447\u0430\u0441 \u043D\u0430 \u043C\u0435\u043D\u044F \u044D\u0442\u043E \u043F\u0440\u043E\u0441\u0442\u043E \u043D\u0435 \u0432\u043B\u0438\u044F\u0435\u0442. \u0415\u0441\u043B\u0438 \u044F \u0447\u0442\u043E-\u0442\u043E \u0440\u0438\u0441\u0443\u044E, \u0442\u043E \u044D\u0442\u043E \u0447\u0442\u043E\u0431\u044B \u0440\u0430\u0437\u0431\u0430\u0432\u0438\u0442\u044C \u0442\u0438\u0448\u0438\u043D\u0443 \u0432 \u044D\u0442\u043E\u043C \u0442\u0440\u0435\u0434\u0435. \u0412\u0441\u0435 \u0431\u043E\u043B\u044C\u0448\u0438\u0435 \u0440\u0430\u0431\u043E\u0442\u044B \u043A\u043E\u0442\u043E\u0440\u044B\u0435 \u044F \u043D\u0430\u0447\u0430\u043B, \u043D\u043E \u043D\u0435 \u0437\u0430\u043A\u043E\u043D\u0447\u0438\u043B ,\u043A\u0430\u0436\u0443\u0442\u0441\u044F \u043C\u043D\u0435 \u0431\u0435\u0437\u043D\u0430\u0434\u0451\u0436\u043D\u044B\u043C\u0438. \u0425\u043E\u0442\u044F \u0441\u043A\u043E\u0440\u0435\u0435 \u044F \u043F\u0440\u043E\u0441\u0442\u043E \u043B\u0435\u043D\u0442\u044F\u0439.\n\u003C/p\u003E\n\u003Cp\u003E\n\u0421\u0435\u0439\u0447\u0430\u0441 \u044F \u043F\u0435\u0440\u0435\u043A\u0438\u043D\u0443 \u0432\u0441\u0451 \u0441 \u0430\u043D\u0433\u043B\u0438\u0439\u0441\u043A\u043E\u0433\u043E \u0442\u0440\u0435\u0434\u0430 \u0438 \u043D\u0430 \u044D\u0442\u043E\u043C \u043D\u0430\u0434\u0435\u044E\u0441\u044C \u0437\u0430\u043A\u043E\u043D\u0447\u0438\u043C.\n\u003C/p\u003E\n\u003Cp\u003E\n\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u00A0\n\u003C/p\u003E\n\u003Cp\u003E\n\u044B\n\u003C/p\u003E\n\u003Cp\u003E\n\u003Cimg alt=\u0022ra3rl0.jpg\u0022 src=\u0022http://oi68.tinypic.com/ra3rl0.jpg\u0022 style=\u0022width:300px;height:auto;\u0022\u003E\u003C/p\u003E\n\u003Cp\u003E\n\u003Cimg alt=\u0022\u0026lt;br/\u0026gt;\u0026lt;a href=\u0026quot;http://oi67.tinypic.com/2zpmhpg.jpg\u0026quot; target=\u0026quot;_blank\u0026quot;\u0026gt;View Raw Image\u0026lt;/a\u0026gt;\u0022 src=\u0022http://oi67.tinypic.com/2zpmhpg.jpg\u0022 style=\u0022width:300px;height:auto;\u0022\u003E\u003C/p\u003E\n\u003Cp\u003E\n\u003Cimg alt=\u0022\u0026lt;br/\u0026gt;\u0026lt;a href=\u0026quot;http://oi